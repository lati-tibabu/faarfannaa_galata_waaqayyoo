  "category": "worship",
  "sections": [
    {
      "type": "CHR",
      "lines": [
        "Waaqayyoon abdii godhadhaa/2/",
        "Cimaa garaan keessan hin nain/2/"
      ]
    },
    {
      "type": "VRS1",
      "lines": [
        "Waaqayyo humna qabeessa,",
        "Inni jaalala qabeessa,",
        "Inni Gooftaa hundumaa ti,",
        "Inni mootii ulfinaa ti."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Waaqayyoon abdii godhadhaa/2/",
        "Cimaa garaan keessan hin nain/2/"
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Yaa ijoollee Gooftaa jabaadhaa,",
        "Diina balbala dhowwadhaa,",
        "Abdiidhaan deemaa didichaa,",
        "Amantii fiixaan baafachaa."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Waaqayyoon abdii godhadhaa/2/",
        "Cimaa garaan keessan hin nain/2/"
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Maqaa Gooftaatiin jiraadhaa,",
        "Garaa keessan jabeeffadhaa,",
        "Hin sodaatinaa yeroo hundaa,",
        "Gooftaan in dhufaa gammadaa."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Waaqayyoon abdii godhadhaa/2/",
        "Cimaa garaan keessan hin nain/2/"
      ]
    }
  ]
