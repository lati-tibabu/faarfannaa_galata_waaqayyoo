{
  "number": 239,
  "title": "Gaaddisa fi da'oo Abbaatti",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Gaaddisa fi da'oo Abbaatti",
        "kan jiraatu,",
        "Isa boqonnaa kan argatu,",
        "Waaqayyoo Gooftaa irkoo",
        "kan godhatu,",
        "Inni hin gufatu hin kufu."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Siif siif abbooma,",
        "Si eeguufis ergamoota isaa",
        "Siif abbooma,",
        "Wanti hamaan",
        "si hin miidhu gonkumaa.",
        "Diinni kee si adamsa kiyyoo",
        "baayyee qaba,",
        "Maaf duuta nama baqadhu,",
        "Waaqayyoo qoochoo isaatiin",
        "si golbooba,",
        "Gaachanaa fi da'oo kee godhadhu."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Bitaa fi mirgaa keetti yoo",
        "kukkufan illee,",
        "Kumaatamootni diinoti kee,",
        "Sibira hin ga'u in ilaalta malee,",
        "Gaatii cubbamootaa in argata."
      ]
    },
