{
  "number": 229,
  "title": "Yesus tiksee tolaan,",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yesus tiksee tolaan,",
        "Kan nama hin dagatne,",
        "Gumaa lubbuu kootii,",
        "Lubbuu isaa naaf kenne."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Iddoo qopheessitee,",
        "Anaaf mana Abbaa kootti,",
        "Du'a na oolchite ,",
        "Situ Gooftaa kooti."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Si malee hin waammadhu,",
        "Hin barbaadu homaa iyyuu,",
        "Barbaadu hin argadhu,",
        "kan sirra naaf wayyu."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Maqaa Yesus malee,",
        "Kan bira hin argamne,",
        "Akka ittiin fayyinuuf",
        "Kan Abbaan nuuf kenne."
      ]
    },
    {
      "type": "VRS5",
      "lines": [
        "Qulqulluu dhiiga isaa,",
        "Aarsaa godhee kennee,",
        "Kan itti amananiif,",
        "Karaa waaqaa bane"
      ]
    }
  ]
}
