  "title": "Dhumni barichaa dhiyaateera,",
  "category": "gospel",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Dhumni barichaa dhiyaateera,",
        "Kan Gooftaan dubbates",
        "Raawwateera,",
        "Qalbii diddiirradhaa",
        "Yaa ilmaan namaa,",
        "Guyyichi fayyinaas",
        "Kunoo ammumaa/2/"
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Dhufni Mootichaa",
        "Kunoo in dhi'aatee,",
        "Qophooftee taa'i",
        "Ifsaa kee guuttee,",
        "Yommuu inni dhufu",
        "Isaa simuudhaaf,",
        "Gammachuu Gooftaatti",
        "Wajjin galuudhaaf/2/"
      ]
    },
