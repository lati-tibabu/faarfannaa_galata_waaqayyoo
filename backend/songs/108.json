{
  "number": 108,
  "title": "Maqaa Sadan Tokkummaatiin",
  "category": "worship",
  "sections": [
    {
      "type": "CHR",
      "lines": [
        "Maqaa Sadan Tokkummaatiin",
        "Cuuphameera/2/",
        "Gooftaa yesusiin",
        "amanee haara'eera./2/"
      ]
    },
    {
      "type": "VRS1",
      "lines": [
        "Namummaa isa moofaa",
        "ani gateera,",
        "Goofata yesusiin,",
        "Jireenya dukkanaa sana",
        "ani dhiiseera,",
        "Harka seexanaichaa keessaa",
        "ani ba'era."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Maqaa Sadan Tokkummaatiin",
        "Cuuphameera/2/",
        "Gooftaa yesusiin",
        "amanee haara'eera./2/"
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Yaaddoo koo hundaas",
        "Narraa fuudheera,",
        "Garaan koo dhiiga",
        "isaatiin dhiqameera,",
        "Firdii badiisaa na oolche",
        "Yesus koo,",
        "Egaa anoo isa hin dhiisu",
        "fayyisaa koo."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Maqaa Sadan Tokkummaatiin",
        "Cuuphameera/2/",
        "Gooftaa yesusiin",
        "amanee haara'eera./2/"
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Xuraa'ummaa biyya lafaa",
        "Ani dhiiseera,",
        "Huwwii fi kajeellaa fooniitti",
        "qullaa'eera,",
        "gargaaraan koo gooftaa yesus",
        "an fureera,",
        "Gammachuudhaan mana isaatti",
        "na galcheera.."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Maqaa Sadan Tokkummaatiin",
        "Cuuphameera/2/",
        "Gooftaa yesusiin",
        "amanee haara'eera./2/"
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Waaqayyoof galanni haa ta'u",
        "uumaa kootiif",
        "Yesusiif galanni haa ta'u fayyisaa koof,",
        "Hafuura qulquqlluudhaafis",
        "geggeessa koof,",
        "sadan tokkummaa waaqayyoof",
        "hooqubaa issaf."
      ]
    },
