  "title": "Maqaa Abbaa fi Ilmaa",
  "category": "worship",
  "sections": [
    {
      "type": "CHR",
      "lines": [
        "Maqaa Abbaa fi Ilmaa",
        "Hafuura Qulqulluutti,",
        "Abboommii Waaqayyoon",
        "bishaan qabametti,",
        "Namni erga cuuphamee",
        "maatii Gooftichaa ti,",
        "Hafuuraan dhalatate",
        "mucaa Waaqayyoo ti"
      ]
    },
    {
      "type": "VRS1",
      "lines": [
        "Cuuphaadhaan arganne",
        "dhifamuu cubbuudhaa,",
        "Waaqayyoo in raawatw kakuu",
        "fayyinaa dhaa,",
        "Lammaffaa dhalannee",
        "taaneerra maatii isaa,",
        "Ayyaanuma isaatin inni nu fayyisa."
      ]
    },
