{
  "number": 55,
  "title": "Lubbuu ko ka'ii faarfadhu,",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Lubbuu ko ka'ii faarfadhu,",
        "Yesus nuuf rarraafamee;",
        "Nuuf dhiphate nuufis du'e,",
        "Akka hoolaa qalamee,",
        "Nuuf awwaala keessaa ka'e,",
        "Amma immoo asi nu bira."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Inni hundumaa nuuf godhe",
        "Jabina keenya ta'e,",
        "Dhiphina keenyas fudhatee,",
        "Yaaddoo hundaa nu oolche;",
        "Du'a isaatiin akka argannuuf,",
        "Ifaa fi nagaa galatas."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Bofni tumamee nu'is baane,",
        "Kan bowwaa du'aa humni",
        "Si'uma sana hidhamani,",
        "Gaafa Ilmii mo'ate",
        "Kan hir'ate amma guute,",
        "Ayyaanas ifatti baase."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Yaa xinnaa fi guddaa faarfadhaa,",
        "Ulfina Waaqayyootiif,",
        "Galannis Ilmaaf haa ta'u,",
        "Hafuura Qulqulluufis,",
        "Waan nuuf godhe hundumaaf,",
        "Baruma baraan, Ameen."
      ]
    }
  ]
}
