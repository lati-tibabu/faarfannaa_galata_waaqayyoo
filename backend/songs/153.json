{
  "number": 153,
  "title": "Waaqayyo nuuf humna keenya,",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Waaqayyo nuuf humna keenya,",
        "Inni gaachana keenya,",
        "Yeroo gaddaatii fi dhiphinaatti Nu'i isa in abdanna,",
        "Mootiin dukkanaa in dhufa,",
        "Dheekkamaa, aaraas,",
        "Ofis in qopheessa",
        "Aangoodhaa fi haxxummaan,",
        "Nu'i garuu hin sodannu."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Humni keenya nu hin ga'u,",
        "Nu'i dafnee in tamsaana,",
        "Garuu namni jabaan jira,",
        "Nu'i isatti in Irkanna,",
        "Maqaa isaas yoo gaafatte,",
        "Kiristos Gooftaa dha,",
        "Ilma Waaqayyootis,",
        "Du'a mo'ateera.",
        "Tessoosaas dhaabbateera."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Biyyi lafaa hammaatu illee,",
        "Seexanootaan guutu illee,",
        "Garuu utuu Gooftaan jiruu,",
        "Nu'i homaa hin sodaannu,",
        "Seexanatti faradamee,",
        "Aangoon isaas dhumee,",
        "Rifeensa tokko'illee nurraa hin fudhatu,",
        "Dubbii tokkoon in kufa."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Abdiin Waaqayyoo dhugaadha",
        "Nu'i isa jabeeffanna",
        "Si achis nu'i mootii waaqaaf",
        "Soda malee in lolla,",
        "Gaafa hunduma gammadaa,",
        "Nu saamanuu illee,",
        "Nu salphisanuu illee,",
        "Dubbii Gooftaatu caala,",
        "Badhaadhummaa lafaa irra."
      ]
    }
  ]
}
