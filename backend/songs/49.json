{
  "number": 49,
  "title": "Yesus naaf jedhee dhiphate",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yesus naaf jedhee dhiphate",
        "Geetesemaaneetti,",
        "Dafqa du'aa xuruurfatee",
        "Yeroo kadhatetti."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Naaf dhiphate siif dhiphate",
        "Gooftaan nuuf dhiphate",
        "Qaraaniyootti;",
        "Nuuf dhiphate"
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Waraansa keenya fudhatee,",
        "Cubbuu keenya baatee,",
        "Akka waan hamaa hojjetee,",
        "Firdiitti dhi'aate."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Naaf dhiphate siif dhiphate",
        "Gooftaan nuuf dhiphate",
        "Qaraaniyootti;",
        "Nuuf dhiphate"
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Wayyaa bildiimmaa uffatee",
        "Fannoo isaa baadhatee,",
        "Gonfoo qoraatti kaa'atee,",
        "Hundaa nuuf raawwate."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Naaf dhiphate siif dhiphate",
        "Gooftaan nuuf dhiphate",
        "Qaraaniyootti;",
        "Nuuf dhiphate"
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Inni angafa lubaa nuuf ta'ee,",
        "Waaqa irraa gadi bu'ee,",
        "Yakka koottii na furuudhaaf,",
        "Kan naaf jedhee du'e."
      ]
    },
    {
      "type": "CHR",
