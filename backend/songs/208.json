{
  "number": 208,
  "title": "Bakkalchichi ba'era,",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Bakkalchichi ba'era,",
        "Halkanichis darbee,",
        "Dhaloonni namaa in ka'u,",
        "Waaqayyoon waaqessuuf,",
        "Kan ergaman hundinuu,",
        "Waan haara nutti himu,",
        "Macca akka jalqabe",
        "Waaqayyoof sagaduu."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Fixeensi gadi in bu'a",
        "Akka tiifuu bokkaa,",
        "Gaafa hundaa raajii haaraa",
        "Iji keenya in arga,",
        "Kadhata nama hundaa",
        "Inni in dhaga'a,",
        "Isa kan amananiif",
        "Hafuurri in kennama."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Kunoo namoonni danuun",
        "Waaqayyoon waaqessu,",
        "Kan kumaatamaa lubbuun",
        "Gara isaa ilaalu,",
        "Baras in ariifatu",
        "Namootni biraa immoo,",
        "Yesusiin barbaaduudhaaf",
        "Isa namaaf du'e."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Dafii dafii bal'adhu",
        "Yaa burqaa ayyaanaa,",
        "Biyya hundumaas ga'i",
        "Garaa hundaa bani,",
        "Utuu Gooftaan hin dhufin",
        "Dafiitii jarjari,",
        "Biyya lafaa qopheessuuf",
        "Isa dhufu sanaaf."
      ]
    }
  ]
}
