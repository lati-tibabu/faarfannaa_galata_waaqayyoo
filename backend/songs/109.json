{
  "number": 109,
  "title": "Kottaa hoo'aa nyaadhaa dhugaa",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Kottaa hoo'aa nyaadhaa dhugaa",
        "Yesus isiniin jedha,",
        "inni isiniif dhufeera",
        "dhiphinas dhiphateera,",
        "kan beelofuu fi kan dheebottu,",
        "Amma kottuu fudhadhu,",
        "Nyaadhuu dhugi gatii malee,",
        "waan barbaadddu hundumaa."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Gara kee ani dhufuun fedha,",
        "Yaa Goofaa yesus kiristos,",
        "fudhaadhee nyaachuus nan fedha,",
        "waan ati nan kennitu irraa,",
        "Waaqayyo kenna kee hundaa,",
        "Fayyina koof naaf naaf godhi,",
        "garaa koo na jajjabeessi,",
        "Lubbuu koo naf haaressi."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Gara koo kottu yaa yesus",
        "Namummaa koo dhuunfadhu,",
        "ani si malee homaa hin qabu,",
        "fayyinaa koof kan ta'u",
        "dubbii kee duwwaatti amanee",
        "Gara keetti nan dhi'aadha,",
        "achitti akkam argamuttis,",
        "humna keetiin na eegi."
      ]
    }
  ]
}
