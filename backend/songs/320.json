{
  "number": 320,
  "title": "Mana Iseey Beeteliheemichatii,",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Mana Iseey Beeteliheemichatii,",
        "Maatii isaa keessaa iddoo",
        "horii tikaatii,",
        "Saamu'eel Daawitiin waamsisee,",
        "Hafuura Qulqulluun dibee",
        "moosise."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Gooftichi beeka hojjetaa isaa,",
        "Iddoo fedhee waamee",
        "addaan baasa,",
        "Namno fuula nama ilaala,",
        "Waaqayyoo garuu garaa ilaala."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Hafuurri Waaqayyoo isa irraa",
        "boqotee,",
        "Hojii itti waametti ergate,",
        "Daawiit inni tiksee horii ture,",
        "Uummata isaa harka diina",
        "isaatii fure."
      ]
    },
    {
      "type": "CHR",
