{
  "number": 186,
  "title": "Yesus na waameera",
  "category": "salvation",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yesus na waameera",
        "furin lubbuu kootii,",
        "Maal fudhadheen dhaqa",
        "fuula goofta kootii."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Maal dhi'eessaree",
        "gati cubbuu kootii,",
        "Meetii moo warqeedha",
        "furiin lubbuu kootii."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Yeus naaf du'eera",
        "Gumaa lubbuu kootii,",
        "Isatu naaf baate",
        "Ba'aa cubbuu kootii."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Yaa Yesus nangatiin",
        "anoo kophaa kootii,",
        "Si malee hin jiruu",
        "Abdiin lubbuu kootii."
      ]
    }
  ]
}
