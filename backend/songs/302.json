{
  "number": 302,
  "title": "Gooftaa nu uumetti gammadaa,",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Gooftaa nu uumetti gammadaa,",
        "Yaa ijoollee namootaa,",
        "Fagoodhaa fi dhi'oodhaa kottaa,",
        "Isa kan jaallattan,",
        "Kottaa hundumti keessan,",
        "Ittis sonaan gammadaa."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Isa'oo kan nu uummate,",
        "Nu'i hoolota isaati'oo,",
        "Kan bade dhaqee barbaada,",
        "Tikseen keenya isa'oo,",
        "Nu geessuus in fedha'oo",
        "Gara gammachuu waaqaatti."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Waaqayyoon galateeffadhaa,",
        "Guddina isaas odeessaa,",
        "Lubbuu kan qabu hundinuu,",
        "Waaqayyoof haa sagadu,",
        "Uumaan keenya arjaa dha,",
        "waan jedhes fiixaan baasa."
      ]
    }
  ]
}
