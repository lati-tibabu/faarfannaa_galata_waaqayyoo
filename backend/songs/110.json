{
  "number": 110,
  "title": "Amma nan gammada",
  "category": "salvation",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Amma nan gammada",
        "Gammachuu argadheera,",
        "Mootii nagaa biraa",
        "Inni na yaadateera,",
        "Yesus fayyisan koo",
        "Hundumaas naaf kenna",
        "Waan lubbuun koo fedhu",
        "Badhaadhummaa isaa irraa."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Yaa seexanaa fi cubbuu",
        "Isin hamoota  nana,",
        "Fagaadhaa ana irraa",
        "Waaaqayyoo naaf birmaata,",
        "Garaa koo in jabbessa",
        "Eenyu na tuqa ree,",
        "Utuu inni naaf jiruu",
        "Gaachanni koo isa'oo."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Isatu jaallate",
        "Mucaa isaa na godhate,",
        "Funyoo seexanaattis",
        "Boolla du'aas na olchee,",
        "Ofii na furaate.",
        "Egaa na barsiisi,",
        "Waaqayyo goofta ko",
        "Akkan si jaalladhuf."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Humna kee naaf kenni,",
        "Seera kee akkan eeguutti",
        "Waan ati hin jaallannes",
        "Animmoo akkan jibbutti,",
        "Gooftaa na gargaari",
        "Gammachuu hundumaan,",
        "Maqaa kee sodaataa",
        "Akkan siif jiraadhu."
      ]
    }
  ]
}
