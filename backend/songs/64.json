{
  "number": 64,
  "title": "Yaa Yesus ani nan dhufa,",
  "category": "salvation",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yaa Yesus ani nan dhufa,",
        "Gidiraa koo wajjiniin,",
        "Hammeenyi koo guddaadhaa,",
        "Yaa Gooftaa na fayyisi."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Dafii na bira ga'i,",
        "Cubbuu koo naaf balleessi,",
        "Garaa kootif yaada koo,",
        "Dhiiga keetin qulleessi."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Waanan qabu hundumaa,",
        "Foon kootif namummaa koo,",
        "Harka keettan kennadha,",
        "Naa fudhadhu gooftaako."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Beekamaa dubii keetiin,",
        "Diinota koo nan mo'a,",
        "Isaan natti jabaatan,",
        "Atimoo naaf birmatta."
      ]
    },
    {
      "type": "VRS5",
      "lines": [
        "Yesusis araarsaa koo ti,",
        "Eenyu inni na dhiphisu?",
        "Anoo isan eeggadhaa,",
        "Innis nan iirranfatu"
      ]
    },
    {
      "type": "VRS6",
      "lines": [
        "Maqaadhuma kee kophaa,",
        "Amanadheen jiraadha",
        "Anis si duukkaan bu'aa,",
        "Adaraa kee nan gatin."
      ]
    }
  ]
}
