  "title": "Lafti nuuf bari'e galata kee Gooftaa",
  "category": "praise",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Lafti nuuf bari'e galata kee Gooftaa",
        "galata kee,",
        "Ummati kee hundinuu",
        "afaan in saaqqatu galata keef,",
        "Indaaqqoon iyyitee",
        "weenniinis korrisee galata keef,",
        "Simbirroonni faarfatanii aduunis",
        "nuu ifse galata keef."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Galata kee Gooftaa",
        "waa'ee guyyaadhaa fi halkanii,",
        "Waa'ee ganamaa fi galgalaa",
        "yeroo dhufani darbanii,",
        "Ulfina kee fi galata keef",
        "kan uummatte namoonni kee,",
        "Macca lafaatii wajjiniin",
        "haa dubbatan galata kee",
        "Nurra haa jiraatu eegumsi kee",
        "nu wajjin haa turuu ergamaan kee."
      ]
    },
