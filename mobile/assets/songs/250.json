{
  "number": 250,
  "title": "Manni keenya abdiin Waaqa irraa",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Manni keenya abdiin Waaqa irraa",
        "Kan Gooftaan Yesus",
        "Nuuf qopheesse,",
        "Nus achitti galuudhaaf jirra,",
        "Biyya keessummummaatii baanee."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Gaafas aduun tolaa,",
        "Qulqullootaaf ba'uudhaaf jira,",
        "Gaafas aduun tolaa",
        "Qulqullootaaf ba'uudhaaf jira."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Iddoon dhaala keenyaas achi'oo,",
        "Kan nu'i amantiidhaan",
        "Eeggannuu,",
        "Maqaa Yesusiin arganneerra,",
        "Bara baraan akka achi teenyu."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Gaafas aduun tolaa,",
        "Qulqullootaaf ba'uudhaaf jira,",
        "Gaafas aduun tolaa",
        "Qulqullootaaf ba'uudhaaf jira."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Fayyisaan keenya dhufuuf jira,",
        "Kanaafis kottaa in qophoofnaa,",
        "Abdii keenyattis in jabaatnaa,",
        "Dhufa isaattis guddaa gammadnaa."
      ]
    },
    {
      "type": "CHR",
