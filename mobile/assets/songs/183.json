{
  "number": 183,
  "title": "Gara kootti iyyi siifan deebisaa,",
  "category": "worship",
  "sections": [
    {
      "type": "CHR",
      "lines": [
        "Gara kootti iyyi siifan deebisaa,",
        "kan ati hin beekne sin agarsiisaa,",
        "humna haaraas sin hidhachiisa,",
        "Eebba koo irraas siin shangalaasa.*2"
      ]
    },
    {
      "type": "VRS1",
      "lines": [
        "Ani uumaa keetii atis",
        "mucaa koo,",
        "Hin irraanfattee",
        "Abbaa kee ta'uu koo,",
        "Gara kootti iyyi ani siifan dhaga'aa,",
        "Kan na gaafattef deebii siin ta'a.*2"
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Gara kootti iyyi siifan deebisaa,",
        "kan ati hin beekne sin agarsiisaa,",
        "humna haaraas sin hidhachiisa,",
        "Eebba koo irraas siin shangalaasa.*2"
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Maaf abdii dhabda ati mucaa ko,",
        "Abdiin kee anaa kottuu gara koo,",
        "Kufeera jette maaliif raafaamta?",
        "Kottu gara koo humna argattaa.*2"
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Gara kootti iyyi siifan deebisaa,",
        "kan ati hin beekne sin agarsiisaa,",
