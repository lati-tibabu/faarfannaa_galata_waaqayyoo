{
  "number": 103,
  "title": "Gooftaa ijoollee keenya,",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Gooftaa ijoollee keenya,",
        "Gara keetti dhi'eesina,",
        "Dubbii kee dhugaatti amannee,",
        "Maqaakeetiin isaan cuuphna"
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Jaalala keetiin simadhu,",
        "Eebba kee isaaniif kenni,",
        "Gara jireenyatti geessi,",
        "Mootummaa keetiif qopheessi."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Hafuura Qulqulluudhaanis,",
        "karaa fayyinaa irratti eegi,",
        "Fuula Waaqayyoo duratti,",
        "Araara haa argatanuu."
      ]
    }
  ]
}
