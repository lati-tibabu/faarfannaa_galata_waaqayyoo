{
  "number": 94,
  "title": "Yeroo walitti sassaabamnu,",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yeroo walitti sassaabamnu,",
        "Dubbii kee dhaga'uudhaaf,",
        "Ayyaana kee irraa nuuf kenni,",
        "Kan hirribaa nu kaasu,",
        "Gurra lubbu keenyas bani,",
        "Hafuura keen nu ho'isi,",
        "Gaarummaadhaanis nu quufsi,",
        "Yaa Gooftaa maaddii kee irraa,",
        "Gaarummaadhaanis nu quufsi,",
        "Yaa Gooftaa maaddii kee irraa."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Akka eegnu nu barsiisi,",
        "Kan nuuf kenname dubbii,",
        "Ifa keetiinis nu geessi,",
        "Karaa gara kee geessutti,",
        "Qalbii keenya nuuf diddiirri,",
        "Waan jal'inaa akka hin yaadnetti,",
        "Yaa Waaqayyo nu gargaari,",
        "Jaalala kee gochuudhaaf,",
        "Yaa Waaqayyo nu gargaari,",
        "Jaalala kee gochuudhaaf."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Eeguudhaafis nu gargaari,",
        "Yeroo ati nu barbaaddu,",
        "Guyyaan ayyaanaa in darba,",
        "Kanaa achi nu hin waamtu,",
        "Amma akka dhageenyu godhi,",
        "Lubbuu keenyaa kan nuuf ta'u,",
        "Gurra qalbii keenyaas bani,",
        "Iyya keenyas dhaga'i,",
        "Gurra qalbii keenyaas bani,",
        "Iyya keenyas dhaga'i."
      ]
    }
  ]
}
