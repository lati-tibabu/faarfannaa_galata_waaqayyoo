{
  "number": 316,
  "title": "Yesus jajjabeessaan koo",
  "category": "salvation",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yesus jajjabeessaan koo",
        "Anaaf du'atii of kenneera,",
        "Isa hordofuun fedha,",
        "Macaafa isaati na caafeera,",
        "Maqaan kooa beekameera,",
        "Fuula Waaqayyoo duratti."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Egaa ani hin barbaadu",
        "badhaadhummaa biyya lafaa,",
        "Warqeenii fi Meetiin maali",
        "Dhagaa dhaa fi",
        "biyyoo dha malee,",
        "Dafaniis in harca'u,",
        "Yesus garuu in jiraata."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Inni anaaf fayyina",
        "Garasaa dhaquus nan hawwa,",
        "Inni anaaf in salphisa",
        "Dhiphina namummaa kooyis,",
        "Aitti hunduu rakkoo dha,",
        "Yesus garuu na'oolcha"
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Yaa biyya lafaa nana,",
        "Na sossobuu kan barbaadu,",
        "Kajeellaa hamaa fi cubbuu,",
        "Xuraa'uummaa kee wajjiinis,",
        "Namummaa koo irraa beenu,",
        "Ani hoomaa sitti hinqabuu."
      ]
    }
  ]
}
