{
  "number": 42,
  "title": "Gooftaa koo yesusiin",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Gooftaa koo yesusiin",
        "baay'ee dhiphisan",
        "Jirmicha fannoodhaa",
        "kophaa baachisan",
        "Gooftaa kabalanii danuu itti qoosan",
        "Anaaf siif anaaf siif *4"
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Yesus yakka malee akkana dhiphate",
        "Gargaarsa hin arganne",
        "Abbaa isatti himatee",
        "Fayyiinni lubbuu koo",
        "isumaan waan ta'eef",
        "Anaaf siif anaaf siif *4"
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Muka fannoo irratti",
        "yommuttii dhiphate",
        "Lubbuun kessaa ba'uuf",
        "yommuttii muddame",
        "nan dheebodhe jedhee",
        "eegdotattii himate",
        "Anaaf siif anaaf siif *4"
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Xoofoo isaaf qophaa'ee",
        "dhugee erga fixee",
        "Waa'ee keenyaaf jedhee",
        "du'ee awwaalamee",
        "Du'a mo'ee ka'ee yaaddoo nurraa fixe",
        "Anaaf siif anaaf siif *4"
      ]
    }
  ]
}
