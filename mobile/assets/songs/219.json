  "title": "Karaa Xiyoon utuu deemnuu",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Karaa Xiyoon utuu deemnuu",
        "Gidiraan yoo nu qaqqabe,",
        "Gooftaa Yesus abdiin keenya",
        "Keessaa nu baasa harka qabee,",
        "Yoo jibbamne yoo miidhamnes,",
        "yoo reebamne, yoo hidamnes,",
        "Gaaf tokko garuu in boqonna,",
        "Gooftaa keenya bira geenyee."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "In jabaanna jajjabaadhaa",
        "Obsaan eeggannaa abdii keenya,",
        "Biyyi lafaa nu gowwoomsee",
        "Akka hin dhifnee Goofta keenya."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Biyya lafaatti har'a miti",
        "Keessummoota ta'uun keenya,",
        "Gooftaa wajjin bara-baraan",
        "Jiraachuu dha bu'aan keenya,",
        "Wal yaaduun iddoo hin jirre,",
        "Biyya qulqulluu jiraanna,",
        "Gaddi keenya amma in darba,",
        "Gammachuudhaan in guutamna."
      ]
    },
