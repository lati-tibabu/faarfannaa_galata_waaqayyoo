{
  "number": 258,
  "title": "Duuka buutuun Gooftaa",
  "category": "praise",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Duuka buutuun Gooftaa",
        "Keenyaa",
        "Jajjabaadhaa achi deemnaa,",
        "Bishaan du'aa sana ceenaa,",
        "Iddoo dhugaa sana geenyaa."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Ka'iitii faarfadhu egaa,",
        "Yesus Kiristos si eegaa,",
        "Bara jirtu hundumaattuu,",
        "Waa'ee isaa himi hundaattuu."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Egaa gammadii lubbuu ko,",
        "Sitti fagaate cubbuun kee,",
        "Cubbuun kee sonaan guddaa dha,",
        "Kan si fayyise Gooftaadha."
      ]
    }
  ]
}
