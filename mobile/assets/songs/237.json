{
  "number": 237,
  "title": "Waaqayyo tiksee kooti'oo,",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Waaqayyo tiksee kooti'oo,",
        "Waan tokko illee hin dhabu,",
        "Kaloo margaa irras na ciibsa,",
        "Waan hamaa hin sodaadhu,",
        "Gara bishaan taliilaattis,",
        "Ofii isaatiin na geessa."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Waaqayyo tiksee kootii dha,",
        "Waan hamaa hin sodaadhu.",
        "Lubbuu koos inni in deebisa,",
        "Karaa tolaatti na geessa,",
        "Inni maqaa isaatiif jedhee,",
        "Waan hamaattii na eega,",
        "Laga du'aa keessa illee,",
        "Waan hamaa hin sodaadhu."
      ]
    },
    {
      "type": "OTR",
      "lines": [
        "Ati na wajjiin jirta'oo,",
        "Waan hamaa hin sodaadhu.",
        "Uleen keetii fi dhaabanni kee,",
        "Anaan in jajjabeessu,",
        "Diinota koo fuul durattis,",
        "Maaddii naaf qopheessitee,",
        "Mataa koos dhadhadhaan dibdee,",
        "Xoofoon kooyis irraa hafaa dha."
      ]
    },
    {
      "type": "OTR",
      "lines": [
        "Waaqayyoo tiksee kooti'oo,",
        "Waan hamaa hin sodaadhu.",
        "Dhugumaan tollii fi araarrii,",
        "Hunduu kan Waaqayyoo ti,",
        "Mana Waaqayyoo keessattis,",
        "Na wajjin in jiraata,",
        "Iddoo gaarii qulqullaa'aas,",
        "Waaqayyoo na teessisa."
      ]
    },
    {
      "type": "OTR",
      "lines": [
        "Waaqayyoo tiksee kooti'oo,",
        "Waan hamaa hin sodaadhu."
      ]
    }
  ]
