    {
      "type": "VRS1",
      "lines": [
        "Cubbun koo baay'ee dha",
        "uumaa koo",
        "Adaraa naaf dhiisi mootii koo;",
        "Si yakkeera guutuu bara koo,",
        "Natti araaram Gooftaa ko.",
        "na maari kaa na maari kaa,",
        "sin yakke uumaa koo na maari kaa;",
        "Na maari kaa na maari kaa,",
        "Sin yakkee uumaa koo na maari kaa."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Miilli koo badiisatti na geessa,",
        "Yaadni foon kootiis na gowwoomsa,",
        "Sii'olitti na galcha cubbuun koo,",
        "Si duwwaa dha garuu abdiin koo."
      ]
    },
    {
      "type": "CHR",
      "lines": [
