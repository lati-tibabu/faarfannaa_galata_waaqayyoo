{
  "number": 230,
  "title": "Eenyu inni kan hin sodaatne,",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Eenyu inni kan hin sodaatne,",
        "Ijoollee Waaqayyoo malee,",
        "Urjoonni illaa Waaqa keessatti,",
        "Simbirroonnis man'ee isaaniitti."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Gooftaan ijoollee isaa isa soora,",
        "Manicha isa Xiyoon keessatti,",
        "Inni dafee isaan maara,",
        "Harka isaattis isaan baata."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Kan hin lakkaa'amne dhiphinni,",
        "Isattii gargar nu hin baasu,",
        "Inni hundumaa irra wayya,",
        "Horii isaa kan miidhus beeka."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Inni rifeensa lakkaa'a,",
        "Kan mataa keenyatti bu'u,",
        "Inni nyaata nuuf in kenna,",
        "Dhugaatii yommuu dheebonnu."
      ]
    },
    {
      "type": "VRS5",
      "lines": [
        "Ai kan amantu gammadi,",
        "Gooftaa maccaa eegduu keetii,",
        "Ulfina isaa fi surraa isaatiin,",
        "Diinota kee in mo'atta."
      ]
    },
    {
      "type": "VRS6",
      "lines": [
        "Hooqubaan siif yaa Waaqayyoo,",
        "Ani amma nan gammada,",
        "Ayyaana waan naaf keenniteef,",
        "Deebitee kan hin fudhanne."
      ]
    },
    {
      "type": "VRS7",
      "lines": [
        "Galata kee yaa Yesus ko,",
        "Ati gaaf hundumaa na bira,",
        "Dhiigni kee naaf dhangala'e,",
        "Cubbuu dhaa fi du'a fageesse."
      ]
    },
    {
      "type": "VRS8",
      "lines": [
        "Hooqubaan siif haa'ii ta'u,",
        "Gooftaa Hafuura Qulqulluu,",
        "Nu gargaarii nu jabeessi,",
        "Cubbuutti nu qulqulleessi.",
        "9. Barri keenyas yoo dhi'aate,",
        "Lafa kana irraa godaanuuf,",
        "Gara mana keetti nu galchi,",
        "Ameen akkas nuuf haa ta'u."
      ]
    }
  ]
}
