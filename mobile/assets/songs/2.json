{
  "number": 2,
  "title": "Yaa waaqayyo hundi keenya",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yaa waaqayyo hundi keenya",
        "Garaa keenya siif in kennina",
        "Galatas siif dhi'eessina",
        "Humni kee dhuma hin qabu,",
        "Laftii fi waaqni siif dhugaa ba'u",
        "Kan kee hundinuu dhugaadha,",
        "Kirubeel Suraafel",
        "Faarfannaan si jaju,",
        "Hoosaa'inaa yaa qulqulluu",
        "Gooftaa lubbuu,",
        "Humnii fi ogummaan keeti'o"
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Yaa Gooftaa ifa kan jaallattu,",
        "Atoo waan asi jiru iyyuu beektaa,",
        "Ijoollee kee hin irranfattu",
        "Hojii isaaniis hin tuffattu,",
        "Horii kee irree keetti in baatta,",
        "Isaan baattees tabba in baatta,",
        "Ilma kee ergitee isaanis nu furte,",
        "Yeroo hundumaa",
        "Hafuura kee nuuf ergiitii",
        "Isaanis nu geggeessi."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Nu eebbisi nu jabeessi",
        "Ayyaanaan fuula kee mul'isi,",
        "Kan si jaallatanuu hundaaf",
        "Maqaa kee faarfachuu in feena,",
        "Gulanas si galateeffanna",
        "Afaan qulqulluu fi haaraadhaan",
        "Kirubeel Suraafel",
        "Ergamootaa wajjin,",
        "Hoosaa'inaa  yaa qulqulluu",
        "Gooftaa lubbuu,",
        "Humnii fi ogummaanis kan keeti."
      ]
    }
  ]
}
