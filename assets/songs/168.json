{
  "number": 168,
  "title": "Kiristos nu waamaa",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Kiristos nu waamaa",
        "waamicha isaa kottaa,",
        "Hundumaa jaallataa",
        "dhaga'aa namootaa,",
        "Garaa keessan hundaan",
        "Waaqayyotti amanaa,",
        "Erga Wangeela isaa",
        "Jabeessaa hubadhaa."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Isa malee karaan, isa malee",
        "Abdii biraa hin qabnu",
        "Kiristosiin malee,(2)"
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Maqan biraa hin jiru",
        "kan fayyinaaf ta'u,",
        "Namootni lafa irraa",
        "maalumaaf joonja'u,",
        "Fayyina barbaaduuf",
        "olii fi gadi jooru,",
        "Karaa inni dhugaan",
        "Yesus malee hin jiru.",
        "Isa malee ..."
      ]
    },
    {
      "type": "CHR",
