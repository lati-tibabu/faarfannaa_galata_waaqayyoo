{
  "number": 4,
  "title": "Yaa Waaqayyo nu ilaali",
  "category": "salvation",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yaa Waaqayyo nu ilaali",
        "Nu kunoo Siif sagadnaa",
        "Si sodaachuun nuuf in ta'a,",
        "Guddina maqaa keetiif,",
        "Fedhi keenyas guddaa dha.",
        "si arguuf yaa fayyisaa,",
        "Yaa Waaqayyo nu gargaari",
        "Nu'is akka Si fakkaannutti."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Iji kee yaa Uumaa keenyaa,",
        "Iddoo jirrutti nu arga",
        "Waan goonu hundumaa beekta",
        "Macaafa keetti in caafta,",
        "Cubbuu keenya nuuf dhiisi kaa",
        "Maqaa Yesusuuf jettee,",
        "Gaafa guyyaan keenya dhume",
        "Gara Keetti nu fudhadhu."
      ]
    }
  ]
}
