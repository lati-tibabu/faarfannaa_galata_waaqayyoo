{
  "number": 287,
  "title": "Yaa Abbaa keenya fi",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yaa Abbaa keenya fi",
        "Gooftaa keenyaa,",
        "Waaqa irraa kan jirtu,",
        "Mootumman kee haa baay'atu,",
        "Nagaas nuuf haa laatu,",
        "Nagaas nuuf haa laatu."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Maqaa kee isa qulqulluu",
        "hunduu haa waammatu,",
        "Gooftaa kan si amanatu",
        "Hamaan hin argatu,",
        "Hamaan hin argatu."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Jaalalli kee inni Waaqa irraa,",
        "lafa irrattis haa ta'u,",
        "Nuuf kenni soora nu ga'u,",
        "Kan har'a nu ga'u,",
        "Kan har'a nu ga'u."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Yakka keenyas nuuf dhiisi,",
        "Nus akkuma dhiifnu,",
        "Nu hin gatin yeroo qoramnu,",
        "Kophaa humna hin qabnuu,",
        "Kophaa humna hin qabnuu."
      ]
    },
    {
      "type": "VRS5",
      "lines": [
        "Jabeenyi qabeenyi",
        "hunduu dhuufaa kee haa ta'u,",
        "Si'i kan bara baraan mo'u,",
        "Siif galatni haa ta'u,",
        "Ameen ha'iita'u."
      ]
    }
  ]
}
