{
  "number": 88,
  "title": "Gooftaan keenya hin turu",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Gooftaan keenya hin turu",
        "Dafee dhufuu fi jira,",
        "Surraadhaaf galat",
        "Guddaadhaan waaqa irraa;",
        "Gaafas gatii hojii",
        "Keenyaa immoo is biraa,",
        "Addaddaan dhaabannee fudhachuudhaaf jirra."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Har'a yeroon kan itti",
        "dammaqun,Amantii keenyaaf",
        "dhaabannee kan jiraannu;",
        "Gooftaa Yesus malee",
        "fayyisaa hin qabnu",
        "Asi lafa kana irraa",
        "kan ittiin fayyinu."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Ammuma guyyichi",
        "Qalbii diddiirrannaa,",
        "Kan itti dhageenyu",
        "Dubbicha fayyinaa,",
        "Yeroo utuu qabnuu",
        "Kottaa in qophoofnaa,",
        "Garuu yoo dhiboofne",
        "Gaaf tokko in gaabbina"
      ]
    },
