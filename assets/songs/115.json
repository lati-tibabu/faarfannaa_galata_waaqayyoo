  "title": "Laaqii cubbuu",
  "sections": [
    {
      "type": "CHR",
      "lines": [
        "Laaqii cubbuu",
        "keessa gangalannee",
        "Biyya lafaatii fi diinaa fi",
        "foon keenyaan",
        "itti harkifamnee,",
        "Waaqayyoo uumaa",
        "fi hiriyaa keenya",
        "kan gaddisiifne,",
        "Qulqulloofneerre",
        "dhiiga Yesusiin,",
        "Dhiifama argannee",
        "ayyaana isaatiin."
      ]
    },
