  "title": "Maal deebisuuf laata gochaan",
  "category": "worship",
  "sections": [
    {
      "type": "CHR",
      "lines": [
        "Maal deebisuuf laata gochaan",
        "Gooftaa kootii baay'ee narra darba",
        "Gochaan isaa na dhibe",
        "guutee dhangala'e,",
        "Waan biraa nan dhabe",
        "Galata duwwaa dha,",
        "innuu itti xinnaate."
      ]
    },
    {
      "type": "VRS1",
      "lines": [
        "Jireenya kootti kufee",
        "dadhabee utuun jiruu",
        "Naan jedhe Abbaan koo",
        "ka'i yaa mucaa koo",
        "hin badduu utuun jiru,",
        "Mana isaa nadhaabe",
        "qulqullootaa wajjin,",
        "Maalan deebisaree",
        "baay'isee na dinqe na dhibe anaan."
      ]
    },
