{
  "number": 95,
  "title": "Yaa yeroo gaarii kan",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yaa yeroo gaarii kan",
        "Yesus nuuf kennu,",
        "Yommuu dubbii",
        "Ayyaana isaa dhageenyu,",
        "Kan fayyummaa bara baraa",
        "Nutti himu,",
        "Yommuu nutti dhi'aatu,",
        "Yommu nutti dhi'aatu,",
        "Kaasuu fi barsiisuudhaaf",
        "Jajjabeessuufis."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Yommuu warri biyya lafaa",
        "Jooranuus,",
        "Kan kajeellaa foonii",
        "Duukaa bu'anuu,",
        "Nu'i garuu si'a sana",
        "In dhageenya,",
        "Dubbii Gooftaa keenyaa,",
        "Dubbii Gooftaa keenyaa,",
        "Dubbii isaa dhaga'uun",
        "Nu hin nuffisiisu."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Gooftaa Yesus hafuura kee",
        "Nuuf kenni,",
        "Nu dubbii kee fudhachuuf",
        "Humna hin qabnuu,",
        "Jaalalaa fi amantiis",
        "Homaa hin qabnu,",
        "Waan feenu hundumaa,",
        "Waan feenu hundumaa,",
        "Amantiitti jabaachuuf",
        "Ati nuuf kenni."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Dubbii kee dhabnee",
        "gognee baanerra'oo,",
        "Hundumti keenya",
        "Dhalootaa jalqabnee,",
        "Waaqa irraa fi lafa irra",
        "Tokko illee hin jiru,",
        "Kan qalbii diddiru,",
        "Kan qalbii diddiiru,",
        "Kan sitti nu harkisu",
        "Suma nuuf wayya."
      ]
    },
    {
      "type": "VRS5",
      "lines": [
        "Adaraa qalbii keenya",
        "Nuuf deebisi,",
        "Yeroo dhufuuf akka waa argannutti,",
        "Soora lubbuu keenya",
        "Madda kee keessaa,",
        "Nu,i silaa keeti'oo,",
        "Nu'i silaa keeti'oo,",
        "Dhiigaan kan nu bitte",
        "dhuunfaa godhattee."
      ]
    },
    {
      "type": "VRS6",
      "lines": [
        "Egaa yaa Gooftaa",
        "Dafiitii nu maari,",
        "Nu ho'isii du'aafis nu dammaqsi",
        "Hafuura nuuf ergi,",
        "Hafuura nuuf ergi,",
        "Akka nutti mu'atutti,",
        "Kan kee ta'uun keenya."
      ]
    }
  ]
