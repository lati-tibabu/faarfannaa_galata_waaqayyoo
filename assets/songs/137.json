  "title": "Maqaan kee haa ulfaatu",
  "sections": [
    {
      "type": "CHR",
      "lines": [
        "Maqaan kee haa ulfaatu",
        "Yaa Waaqayyo Abbaa,",
        "Kan si amanate nagaa guddaa qaba,",
        "Waa'ee gochaa keetiif",
        "maaluma deebifna?",
        "Waaqaa fi lafa irratti",
        "Ulfaadhu siin jenna."
      ]
    },
    {
      "type": "VRS1",
      "lines": [
        "Karaa badiisaa irraa",
        "waamtee nu deebiftee,",
        "Kan dhuma hin qabne",
        "nagaa kee nuuf laate,",
        "Galanni siif haa ta'u",
        "yaa Waaqayyo Abbaa,",
        "Kan sitti jiraatu",
        "waan hundumaa qabaa."
      ]
    },
