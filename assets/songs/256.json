  "sections": [
    {
      "type": "CHR",
      "lines": [
        "Mudhii keenya dhugaadhaan hidhannee,",
        "Qajeelummaas uffannee dhaabannee,",
        "Amantii keenyas godhannee gaachana,",
        "Mo'icha argachuuf",
        "Gooftaa duukaa in Buuna."
      ]
    },
    {
      "type": "VRS1",
      "lines": [
        "Kaachuu isa jalqabne akka",
        "Hin raawwanne,",
