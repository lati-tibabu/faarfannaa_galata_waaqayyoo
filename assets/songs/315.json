{
  "number": 315,
  "title": "Yesus anaaf hundumaa dha,",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yesus anaaf hundumaa dha,",
        "Kan akka isaas hin jiru,",
        "Egaa akka nama biraa",
        "Isan irraanfadhaaree,",
        "Gargar kan nu baasuu hin jiru",
        "Michuu gaarii kanattii,",
        "Anii fi inni wal-jaallana",
        "Lafa irratti fi waaqa irratti"
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Inni naaf jedhee du'eera",
        "Natti kan faradu hinjiru,",
        "Abbaa isaa anaaf araarame,",
        "Eenyu kan na miidhuu fedhu",
        "kan inni ofii sime,",
        "Eenyu inni kan sodaachisu",
        "Kan inni isa eegu."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "An nan beeka nan amanas",
        "Duuni yookis jireenyi,",
        "Isattii gargar nu hin baasu,",
        "Ergamoonii fi aangonnis",
        "Ol hiiquunis gadi deebiinis,",
        "Gaddis yookiis kan biraa,",
        "Danda'ee gargar na hin baasu",
        "Jaalala Waaqayyoottii."
      ]
    }
  ]
}