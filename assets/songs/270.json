{
  "number": 270,
  "title": "Amantii naaf kenni",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Amantii naaf kenni",
        "Yaa garraamii gooftaa,",
        "Na jabeessi,",
        "Cubbuu koo calaasi",
        "Nagaa kee naaf kenni,",
        "Garaa koo qulleessi",
        "Siif haa ta'uu."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Humna kee naaf kenni",
        "Siif akkan jiraadhu,",
        "Na ho'isi,",
        "Ati akka naaf duute",
        "Na gargaari ana immoo,",
        "Akkan siif hojjedhu",
        "Dubbii dhugaatti."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Diini yoo na marse,",
        "Cubbuun yoo na hidhe,",
        "Ati na oolchi,",
        "Dukkanas naaf ifsi",
        "Gaddas naaf salphisi,",
        "Kiristos naaf birmadhu",
        "Gaafa hundumaa."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Bara dulluma kootti",
        "Duunis yommuu dhufu",
        "Na gargaari,",
        "Yesus Amaanu'eel,",
        "Kottuu na fudhadhu",
        "Bobaa keetti na baadhu",
        "Yaa mootii ko."
      ]
    }
  ]
}