  "title": "Irkoon koo yesusii dha",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Irkoon koo yesusii dha",
        "Bobaa isaa jalattis,",
        "Qabbana nan argadha",
        "Gaarummaan na quufsa,",
        "Faarfannaa nan dhaga'a,",
        "Biyya qulqullaa'aatti,",
        "Waaqayyoof kan faarfatan",
        "Namootni hoofkalan."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Irkoon koo....."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Irkoon koo Yesusii dha,",
        "Guyyaan rakkadhettis,",
        "Guyyaan sosssobamettis,",
        "Inni dhaabata koo ti,",
        "Yaaddoon koo amma in darba,",
        "Dhibeen koos hundinuu,",
        "Lolii dhumuu ga'eera",
        "Imimmaannis hafuu."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Irkoon koo....."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Yesus fayyisaa koo ti,",
        "Inni naaf du'eer,",
        "Gaara koo fi nagaa koottis,",
        "Abdiin koos isuma,",
        "Obsumaan nan eeggadha,",
        "Hamma halkan dabutti,",
        "Hamman isa argadhuutti,",
        "Lubbuun koo achi hawwa."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Irkoon koo....."
      ]
    }
