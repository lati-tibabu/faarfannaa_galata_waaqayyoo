{
  "number": 148,
  "title": "Waaqayyo akkana biyya lafaa jaallate,",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Waaqayyo akkana biyya lafaa jaallate,",
        "Tokkicha Ilma isaa",
        "hamma kennutti, isatti",
        "kan amanu hundinuu akka hin badnetti",
        "Jireenya bara-baraa",
        "haa qabaatuuf malee."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Waaqayyo Goooftaa kee jaalladhu,",
        "Gaaraa kee hundumaan",
        "lubbuu kee hundumaanis,",
        "Yaada kee hundumaanis,",
        "Humna kee hundumaanis,",
        "Kunis hundumaa irra kan caalu",
        "abboommii dha jedhe."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Inni lammaffaan kan akkaisaa akkasi",
        "Hiriyaa kee jaalladhu",
        "akka lubbuu keetti,",
        "Abboommiin biraa hin jiru",
        "isaan kana irra, kan caalu",
        "Akkas Yesus isaaniin jedhe."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Gara koo kottaa dadhaboota nana,",
        "Hundinuu ba'aan keessan kan ulfaate",
        "Anis isinan boqochiisaa",
        "Waanjoon koo, gaarii dha'oo,",
        "Ba'aan kooyis salphaa dha'oo."
      ]
    }
  ]
}