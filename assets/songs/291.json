  "title": "Araar, araar, araar,",
  "sections": [
    {
      "type": "CHR",
      "lines": [
        "Araar, araar, araar,",
        "yaa Waaqayyo araara,",
        "Maaloo nutti araarami araar,",
        "Araar yaa Waaqayyoo",
        "nutti araaramii,",
        "Garaa qulqullaa'aa",
        "dafiita nuuf uumii."
      ]
    },
    {
      "type": "VRS1",
      "lines": [
        "Cubbuu keenya ilaalte",
        "ati nu hin balleessin araar.",
        "Jal'inaan guutuudha",
        "garaan namootadha araar,",
        "Namaa fi nama giddutti",
        "dinumman cimeeraa,",
        "Maaloo nagaa buusi",
        "yaa Abbaa araaraa."
