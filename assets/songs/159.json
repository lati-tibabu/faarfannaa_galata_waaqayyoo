{
  "number": 159,
  "title": "Aduun kee kunoo ol ba'e,",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Aduun kee kunoo ol ba'e,",
        "Yaa Waaqayyo Abbaa,",
        "Ani sin galateeffadha",
        "Guddaas nan gammada"
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Aduun kee hundumaaf ba'a,",
        "Gaaridhaa fi hamaadhaa,",
        "Obsa kee immoo na barsiisi,",
        "Akkan si fakkaadhutti."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Amantii dhugaa naaf kenni,",
        "Si duukaa bu'uudhaaf, Sonaanis sitti gammaduudhaaf,",
        "Baruma baraanis"
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Narraa hin fagaatin yaa Gooftaa,",
        "Sittan baqadha'oo",
        "Baay'isee immoo sin eegadha,",
        "Har'aafuu boriifuu."
      ]
    }
  ]
}