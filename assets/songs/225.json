{
  "number": 225,
  "title": "Kottu Hafuura Qulqullu,",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Kottu Hafuura Qulqullu,",
        "Lubbuu kootiif ifa kenni,",
        "Siifis akkan jiraadhutti,",
        "Ifa jireenyaa naaf kenni,",
        "Karaa qajeelaatti na geessi,",
        "Anoo suman eeggadhaa."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Dhugaa hundumaa na gorsi,",
        "Beekuma qalbii koo bani,",
        "Jaalala Waaqayyooyis,",
        "Guyyaa guyyaan akkan godhutti,",
        "Cubbuutti na qulleessi,",
        "Hamaa keessaa na baasi."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Yaa Gooftaa ko na gargaari,",
        "Sonaan akkan si jaalladhutti,",
        "Jaalala soba hin qabneen,",
        "Gooftaa koo Yesus kiristosiin",
        "Guddisee akkan amanutti,",
        "Inni naaf gaachana'oo."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Guyyaa hundumaa na eegi,",
        "Fittee seexanaa na oolchi,",
        "Yaa'i ergamoota isaas,",
        "Keessa kottis dhugaa ba'i,",
        "Nama Waaqayyoo akkan ta'e,",
        "Kan inni fayyifate."
      ]
    },
    {
      "type": "VRS5",
      "lines": [
        "Ani ofii koo dadhabaa dha,",
        "Kanan kadhadhus hin beeku,",
        "Waan tolus waan hammaatus,",
        "Ati yaa Hafuura Qulqullu,",
        "Akkan jedhu na barsiisi,",
        "Abbaa ko yaa Waaqayyo."
      ]
    },
    {
      "type": "VRS6",
      "lines": [
        "Harka garraamummaa keetiin,",
        "Gaaf hundaaa na jajjabeessi,",
        "Hammenya koo balleessi,",
        "Cubbuu koo narraa fageessi,",
        "Gaarii gochuuf na gargaari,",
        "Seera kees na barsiisi."
      ]
    },
    {
      "type": "VRS7",
      "lines": [
        "Hojii gaarii hojjechuudhaaf,",
        "Humna keetiin na jabeessi,",
        "Gaaf hundaa naaf dabali,",
        "Ogummaa ayyaa kee irraa,",
        "Gorsa kee duukaa na buusi,",
        "Waan sitti mul'ates godhi."
      ]
    },
    {
      "type": "VRS8",
      "lines": [
        "Yeroo yeroo na gargaari,",
        "Ba'eessa akkan hojjedhutti,",
        "Akka hin awwaallettis,",
        "Isa ba'eessa kennaa kee,",
        "Waaqayyoof kan hin hojjenne,",
        "Hojii isaa hin eebbifamuu.",
        "9. Ati yaa hafuura gammachuu,",
        "Gadda kootti na gammachiisi,",
        "Gaafa duuni na waamu,",
        "Ofii jettee na gargaari,",
        "Akka kristaana dhugatti,",
        "Asii akkan godaanutti."
      ]
    }
  ]
}