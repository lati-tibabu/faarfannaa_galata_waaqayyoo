{
  "number": 191,
  "title": "Warqee ba'eessa argadhee,",
  "category": "praise",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Warqee ba'eessa argadhee,",
        "Kan lubbuun koo fedhe,",
        "Eenyu jette yoo gaafatte,",
        "Yesus kiristosidha."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Lubbuu ko ka'ii faarfadhu,",
        "Maqaa fayyisaa kee,",
        "Isa mootiin kee Gooftaan kees,",
        "Galata isaaf galchi."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Inni Gooftaa gooftaataa ti,",
        "Kan moototaas mootii,",
        "Waanjoon isaa sonaan salphaa dha,",
        "Ba'aan isaas akkana."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Ani ofii koo dhibamaa dha,",
        "Sonaanis deegaa dha,",
        "Isatu hundumaa qaba,",
        "waan lubbuun koo fedhu."
      ]
    },
    {
      "type": "VRS5",
      "lines": [
        "Yesus Gooftaan koo abdiin koo,",
        "Gammachuu kooti immoo,",
        "Nagaa isa fi gaarummaa isaanis,",
        "Gaaf hundaa na quufsa."
      ]
    },
    {
      "type": "VRS6",
      "lines": [
        "Inni sooruma kooti'oo,",
        "Bara hundumaafuu,",
        "Guuyaa gaaddisa naaf ta'a,",
        "Halkanis ifa koo."
      ]
    },
    {
      "type": "VRS7",
      "lines": [
        "Inni jaalallee kooti'oo,",
        "Hundumaa irra caala,",
        "Dhibee koo isatu beeka,",
        "Gooftaan koo mootiin koo."
      ]
    },
    {
      "type": "VRS8",
      "lines": [
        "Yaa boqochiisaa garaa ko,",
        "Gooftaa Yesus kiristos,",
        "Jalqabaa fi dhumaa naaf ta'i,",
        "Bara koo hundumaatti."
      ]
    }
  ]
}
