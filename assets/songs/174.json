{
  "number": 174,
  "title": "Yaa jireenya ko maaliif",
  "sections": [
    {
      "type": "CHR",
      "lines": [
        "Yaa jireenya ko maaliif",
        "laafta maaf gadi deemta?",
        "Gooftaa Yesusoo si hirkisuuf",
        "si jabeessuuf si bira dhaabataa."
      ]
    },
    {
      "type": "VRS1",
      "lines": [
        "Hidhaa ati itti hidhamtee jirtu",
        "yoo jiraate",
        "Yesusoo si hiikaa,",
        "Ba'aa sirra jiru wanta sitti ulfatu",
        "gara isaa fidika,",
        "Innoo siin jedheeraa waanuma tokkoofuu",
        "yaaddoo hin qabaatin,",
        "Kan si barbaachisu",
        "Yesusitti himi",
        "homaa iyyuu hin sodaatin."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Yaa jireenya ko maaliif",
        "laafta maaf gadi deemta?",
        "Gooftaa Yesusoo si hirkisuuf",
        "si jabeessuuf si bira dhaabataa."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Hojiin biyya lafaa fi iji hojii foonii",
        "dhumni isaa badiisa",
        "Kan kanaan jiraatu",
        "yeroo hundumaattuu",
        "Gooftaa gaddisiisa,",
        "Har'a bor hin jedhin murtoo",
        "godhachuudhaan",
        "Gooftaatti jabaadhu,",
        "Dhibaa'ummaa malee",
        "jireenya guddatuun",
        "amantiidhaan kaadhu."
      ]
    },
    {
