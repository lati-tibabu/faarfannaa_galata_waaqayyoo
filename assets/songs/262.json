{
  "number": 262,
  "title": "Yaa Gooftaa na simadhu",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yaa Gooftaa na simadhu",
        "Fayyisaa ko ani sittan dheessaa,",
        "Hamaattii na eeggadhu,",
        "Akkan hin badnettti maatii kee",
        "Keessaa."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Gaafa hundaa yeroo hundaa,",
        "Ofii keen na geggeessi,",
        "Ofittis na dhi'eessi,",
        "Dhiiga keetiin na qulqulleessi."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Utuun lafa irra jiruu,",
        "Na geggeessi tarkaanfii kan koo,",
        "Si'ii na wajjiin jiruu,",
        "Ani hin badu yaa fayyisaa ko."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Gaafa hundaa yeroo hundaa,",
        "Ofii keen na geggeessi,",
