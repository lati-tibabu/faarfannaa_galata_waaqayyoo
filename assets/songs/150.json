{
  "number": 150,
  "title": "Yaa Yesus maqaa dinqii,",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yaa Yesus maqaa dinqii,",
        "Hojiin kee nama dinqa,",
        "Yoo dhugaatti amanan,",
        "Jireenyatti nama geessa."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Yesus Kristos dhufee,",
        "Waan guddaa nuuf hojjete,",
        "Isatu nuuf deebise,",
        "Jireenya keenya bade."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Yesus foon keenya uffatee,",
        "Ba'aa keenya nuuf baate,",
        "Kan firdiin nu qabate,",
        "Dhiiga isaatin nu bite",
        "."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Yesus fannoo isaa baatee,",
        "Gara Golgootaa dhaqee,",
        "Cubbuu koo isa baayy'ate,",
        "Dhiiga isaan na irraa dhiqe,"
      ]
    },
    {
      "type": "VRS5",
      "lines": [
        "Balleessaa keenyaaf du'ee,",
        "Awwaala keessa ciisee,",
        "Guyyaa sadaffaatti ka'ee,",
        "Ka'uu nu abdachiise."
      ]
    },
    {
      "type": "VRS6",
      "lines": [
        "Egaa kottaa in amannaa,",
        "Ayyaana isaa akka argannuuf,",
        "Guyyaa dhufa isaa lamuu,",
        "Mirga isaa akka dhaabbannuuf."
      ]
    },
    {
      "type": "VRS7",
      "lines": [
        "Inni ofii isaa jireenya,",
        "Karaa dhaa fi dhugaa dhas,",
        "Kan itti amanu hundinuu,",
        "Hin yeella'u gaafas."
      ]
    }
  ]
}