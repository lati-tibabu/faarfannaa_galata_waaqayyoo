{
  "number": 51,
  "title": "Akka hattuutti qabamee",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Akka hattuutti qabamee",
        "Gooftichi jireenyaa,",
        "Cunqurfamee kabalame",
        "Iddoo hunda keenyaa."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Gaafatanii jedhaniin",
        "Eenyu si rukute?",
        "Fuula kee kan si kabale",
        "Nutti himi jedhaniin."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Inni garuu homaa hin jenne",
        "Kana hundumaattuu,",
        "Yakki afaan isaattii hin argamne",
        "Hamma du'a isaattuu."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Firdiittis isa geessanii,",
        "Ajjeesuu yaadanii,",
        "Irrattis walii galanii,",
        "Haa du'u jedhan."
      ]
    },
    {
      "type": "VRS5",
      "lines": [
        "Innis immoo in abboomame",
        "In baate Fannoo isaa",
        "Dafqa isaa immoo xuruurfatee",
        "Golgootaatti adeeme."
      ]
    },
    {
      "type": "VRS6",
      "lines": [
        "Achiinis isaa ga'anii",
        "Fannootti ol baasanii,",
        "Itti immoo ga'isan,",
        "Gadi of buusi jedhanii."
      ]
    },
    {
      "type": "VRS7",
      "lines": [
        "Innis isaaniif kadhate",
        "Dhiphina isaa obsaa,",
        "Lubbuu isaniitiifis gaddee,",
        "Isaaniif dhiisi jedhe."
      ]
    }
  ]
}
