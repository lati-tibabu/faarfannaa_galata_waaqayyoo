{
  "number": 249,
  "title": "Galata kee Yesus",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Galata kee Yesus",
        "Dubbicha arganeef,",
        "Kan waan hundumaa",
        "Nuuf tolche,",
        "Wanti hundinuu amma",
        "Nuuf toleera,",
        "Akaakuun namaa fayyeera."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Si'achisa yaa Yesus gadi nu",
        "Hin dhiisiin,",
        "Nu wal'aanii nu jabeessi,",
        "Lubbuu si hin beekne immoo",
        "Barbaadii fidi,",
        "Amma iyyuu isheenoo in joortii."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Misirroo kee fudhachuu",
        "Yommuu dhuftu,",
        "Biyya lafaa kana keessaa,",
        "Isheen deebitee fiixumaa",
        "Si hin gantu,",
        "Gammachuu noo gammachuu dhaa."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Ergaa biyya sana geesee",
        "Faarfattee,",
        "Gubbaa sanatti hooqubaa kee,",
        "Gammachuu fi qannootu achitti ta'a,",
        "Dinagdee guddaa isaa wajjin."
      ]
    },
    {
      "type": "VRS5",
      "lines": [
        "Maal maal dinqiidha huursaan",
        "Faarfannaa isaa,",
        "Biyya warra qulqullootaatti,",
        "Achitti namni hin kooru galata isaati,",
        "Haallee Luyyaa yoom achi geenya."
      ]
    },
    {
      "type": "VRS6",
      "lines": [
        "Gara dhumaatti liphii ijaa",
        "Hundumaatti,",
        "Wanti jiru in ariifata,",
        "Galata gammachuu gubbaatti arguudhaaf,",
        "Achi geenyaan egaa in boqonna."
      ]
    },
    {
      "type": "VRS7",
      "lines": [
        "Dhiiga isaatiin nu'i cubbamoonni",
        "In galla,",
        "Yaa namaa nu duukaa bu'i,",
        "Jaalala abbaa duuba galii hin jiruu,",
        "Kan haleelii beekkadhu."
      ]
    },
    {
      "type": "VRS8",
      "lines": [
        "Karaan garuu Yesusiidhaa",
        "Hubadhu,",
        "Yeruusaalemiin kan ga'u kaadhu,",
        "ka gulana gatii fudhachuuf,",
        "Lubbuu ko galtees gammadi."
      ]
    }
  ]
}