{
  "number": 162,
  "title": "Koochotaa kee diriirsi,",
  "category": "salvation",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Koochotaa kee diriirsi,",
        "Yaa Yesus na gubbaatti,",
        "Guyyaa gaarii fi hamaattis,",
        "Ofitti na dhi'eessi,",
        "Jireenya koo naaf ta'i,",
        "Yaa Kiristos gorsaa ko,",
        "Gaaf hundaa ayyaana keen,",
        "Yaa Yesus na eegi."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Cubbuu koo immoo naaf dhiisi,",
        "Dhiiga keen na dhiqi,",
        "Yaada haaraa naaf uumi,",
        "Jaalala  qulqulluus,",
        "Hunda keenya geggeessi,",
        "Bobaa keen nu dhoksi,",
        "Amma immoo nagaa keetiin,",
        "Halkan nu bulfadhu."
      ]
    }
  ]
}
