{
  "number": 16,
  "title": "Waaqa gubbaadhaa nuuf dhufe,",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Waaqa gubbaadhaa nuuf dhufe,",
        "Nagaa kan nutti himu ergan,",
        "Gammachuu guddaa in dhageessu,",
        "Isinis hubadhaa sonaan."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Mucaan isiniif dhalate,",
        "Jaalala isaa akka ture,",
        "Inni durbattii dhalate,",
        "Mucichis Ilma Waaqayyooti."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Inni Nagaa fidee dhufe,",
        "Dur Waaqayyo kan qopheesse,",
        "Warra isa jaallatanuuf,",
        "Mootummaa isaas kan dhaalanuuf."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Inni Nu in jajjabeessa,",
        "Dhiphina keessaas nu baasa,",
        "Cubbuu keenyas nuuf ballessa,",
        "fittee seexanaa nu oolcha."
      ]
    },
    {
      "type": "VRS5",
      "lines": [
        "Yaa Gooftaa ko Yesus Kiristos,",
        "Kadhannaa koo naaf dhaga'i,",
        "Garaa koo keessa jiraadhu,",
        "Yommus nagaa nan argadhaa."
      ]
    },
    {
      "type": "VRS6",
      "lines": [
        "Akkan si hin irraanfannettis,",
        "Yeroo hundumaa na barsiisi,",
        "Maqaa kee akkan Yaadadhu,",
        "Ayyaana kee irraas naaf kenni."
      ]
    },
    {
      "type": "VRS7",
      "lines": [
        "Galanni Abbaaf haa ta'u,",
        "Ilma ofii kan nuuf erge,",
        "Maqaan isaa haa eebbifamu,",
        "Lafa irrattis Waaqa irrattis."
      ]
    }
  ]
}