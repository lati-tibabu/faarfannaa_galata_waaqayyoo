{
  "number": 37,
  "title": "Yesus si galateeffanna",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yesus si galateeffanna",
        "Waan dhiiga kee dhangalaafteef",
        "Haxxummaa fi aangoo diinotaas,",
        "Hurreesitee nu oolchite"
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Yaa Waaqayyo si kadhanna,",
        "Funyoo cubbuu keenyaa kuti,",
        "Yaaddoo boriis nurraa fuudhi,",
        "Gaafa hundumaas nu jabeessi."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Nama hamaattii nu eegi,",
        "Yoo waanjoonis nutti ulfaate",
        "Dafii kottuu nu gargaari",
        "Baâ€™aa keenyas nuuf salphisi."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Yaa Yesus obboleessa ko,",
        "Kottuu na bira jiradhuu",
        "Rakkina keessaa na baasi,",
        "Gara jireenyatti na geessi."
      ]
    }
  ]
}
