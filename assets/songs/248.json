  "title": "Ani biyya ba'eessa beeka,",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Ani biyya ba'eessa beeka,",
        "Biyya dhugaa sana,",
        "Fayyisaanis isaaf ifa,",
        "Aduun isaas hin lixu."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Obsa isaan fannoo kan baatu,",
        "Inni achitti in boqota,",
        "Bara hundumaa gammada,",
        "Yesus bira in taa'a."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Iddoo toloonni jiranuu,",
        "Achi gadduun hin jiru,",
        "Biyyi sun sonaan gaarii dha,",
        "Yoom achi genya ree!"
      ]
    },
