  "title": "Loltuun Qaraaniyoo abban mo'ichaa",
  "category": "praise",
  "sections": [
    {
      "type": "CHR",
      "lines": [
        "Loltuun Qaraaniyoo abban mo'ichaa",
        "Siif jedhee kan baate jirma mukichaa",
        "Irree diina keetii inni si oolchaa,",
        "Kuftee hin argamtu",
        "dhuma barichaatti."
      ]
    },
    {
      "type": "VRS1",
      "lines": [
        "Waldaa Kiristaanaa jajjabaadhu kaa,",
        "Seenaa Gooftaa keetii",
        "hin irranfatin kaa,",
        "Yommuu ari'atamtu",
        "hin yaadda'in kaa,",
        "Dhuma jireenya kee",
        "Inni iyyuu beekaa."
      ]
    },
