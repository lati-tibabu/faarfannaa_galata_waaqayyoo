{
  "number": 192,
  "title": "Kottaa gammachuu gammadnaa,",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Kottaa gammachuu gammadnaa,",
        "Garaa keenyaan ililchaa,",
        "Ayyaanni nuuf kennameeraa",
        "Isa nu uume biraa,",
        "Inni hundati himeera",
        "Akka isaan maaruu barbaadu,",
        "Amma yoo deebi'aniif."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Seexanni na hidhee ture,",
        "Ofis hiikuu hin dandeenye,",
        "Cubbuu kootiinis hidhamee,",
        "Baruma baraan baduuf,",
        "Jabeessee na danqee ture",
        "Abdiin koo hundinuus badee,",
        "Badiisattis dhi'aadhee."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Waaqayyo garuu naaf oo'e,",
        "Dhibee kootti naaf birmatee,",
        "Gidiraa sana na oolche,",
        "Gara isaatti na fudhatee,",
        "Garaa koo naaf qulqulleesse,",
        "Abbaa araaraa naaf ta'e,",
        "Baruma hundumaattuu."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Ilma isaa gadi ergee jedhees,",
        "Ani nama nan jaalladha,",
        "Waan kanaaf ka'iitii bu'i,",
        "Warra hidhaman hiiki,",
        "Isa isaan hidhe cubbuuttii,",
        "Sodaa du'aas isaan oolchi,",
        "Isaan biras jiraadhu."
      ]
    },
    {
      "type": "VRS5",
      "lines": [
        "Ilmis abbaa isaa dhaga'ee,",
        "As gara keenya dhufee,",
        "Ija keenyaan isa argine,",
        "Michuu keenyas nuuf ta'e,",
        "Jabeessees nu jaallate,",
        "Kanaafis waaqaa gadi bu'e,",
        "Dhimma biraas hin qabu."
      ]
    },
    {
      "type": "VRS6",
      "lines": [
        "Anaaf qulqulluu dhiiga isaa,",
        "Fannoo irratti dhangalaasee,",
        "Dheekamsa dhufuuf jiruttii",
        "Akka na hofkalchuuf,",
        "Inni naaf abdii lubbuu kooti,",
        "Du'a isaatiinis ani fayyee,",
        "Har'aaf bara hundumaatti."
      ]
    },
    {
      "type": "VRS7",
      "lines": [
        "Inni gara abbaa isaa dhaqee,",
        "Lafa hamaa kana irraa,",
        "Na jajjabeessuudhaaf jedhee,",
        "Hafuura isaa ergeera,",
        "Inni na jajjabeessa'oo",
        "Waan hundumaas naaf qajeelcha,",
        "jiruu isaa isa kana."
      ]
    },
    {
      "type": "VRS8",
      "lines": [
        "Hooqubaanii fi galanni immoo,",
        "uumaa keenyaaf haa ta'u,",
        "Ilma isaa Yesus Kiristosiin,",
        "Waan gara keenyatti ergeef,",
        "Si'achi cubbuu cala'eera",
        "Kan nu rakkisu eenyu ree?",
        "Nu kan Waaqayyoo ti'oo."
      ]
    }
  ]
}