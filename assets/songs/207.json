{
  "number": 207,
  "title": "Yesus uleen mootummaa kee",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yesus uleen mootummaa kee",
        "Biyya hundaa irra jira,",
        "Ba'a biiftuutii jalqabee",
        "Hamma lixa isheettis",
        "Maqaan kee in beeksifama,",
        "Moototni siif in jilbeenfatu,",
        "Macca hundumaa wajjiin."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Dhala dhalatu hunndumaa",
        "Gara keetti in fiduu'oo,",
        "Gaafa dhibee si barbaadu,",
        "Abdiis hin kutatanuu,",
        "Guyyaa boo'ichaa fi dhukkubaatti,",
        "Mana gaddaa fi iddoo dhibeetti,",
        "Gara kee ilaallatu."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Egaa yaa Yesus kottu kaa,",
        "Yeroo gaarii fi hamaattis,",
        "Hafuura kee dhangalaasi,",
        "Lubbuu hundumaa irrattis,",
        "Hudumaa keenya jabeessi,",
        "Walitti immoo nu sassabi,",
        "Gaafa dhufa keetiitti."
      ]
    }
  ]
}
