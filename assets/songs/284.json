{
  "number": 284,
  "title": "Goofta ija koo naaf bani,",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Goofta ija koo naaf bani,",
        "Seera kee arguudhaaf,",
        "Na eebbisi hafuura keetiin,",
        "Seera akkan eegutti."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Jaalala kee na barsiisi,",
        "Dubbii kee yaaduudhaaf,",
        "Ayyaana kee nan kadhadha,",
        "Akka jaalala keetiitti,"
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Dubbii sobaa irraa na dhowwi,",
        "Siifan abboomamaa,",
        "Yaa Gooftaa siin amanamaa,",
        "Seera kee nan eega."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Jaalala kee na barsiisi,",
        "Dubbii kee yaaduudhaaf,",
