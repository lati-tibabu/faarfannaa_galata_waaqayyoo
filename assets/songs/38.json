{
  "number": 38,
  "title": "Hundumti keessan ilaalaa,",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Hundumti keessan ilaalaa,",
        "Attam Yesus dhiphate,",
        "Garaa guutuudhaan yaadadhaa",
        "Akka inni nuuf rakkate,",
        "Akka dhiphina Yesus",
        "Takkaa dhiphinni hin turre."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Harka isaaf miilla isaa uran,",
        "Namummaan isaas dhiphatee,",
        "Qoraattiin mataa isaa dhigsee,",
        "Fuulli isaas diddirame,",
        "Akka dhiphina Yesus,",
        "Takka dhiphinni hin turrre."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Sagalee guddaadhaan iyyee,",
        "Abbaa isaa dhageessise,",
        "Akkanas Abbaa isaan jedhee,",
        "“Lubbu koo naa fudhadhu”",
        "Akka dhiphina Yesus",
        "Takkaa dhiphinni hin turre"
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Kunoo hojii isaa raawwate,",
        "Dinqiin guddaan isaa ta’e,",
        "Biftuun jaamtee gaarri dho’e,",
        "Guddaa xinnaa hubadhaa,",
        "Akka jaalala Yesus",
        "Takkaa jaalalli hin turre"
      ]
    },
    {
      "type": "VRS5",
      "lines": [
        "Yaa dhala nama ilaali",
        "Gidiraa baay’ee kana,",
        "Cubbuu kee isatti himadhu",
        "Innoo kanaaf du’ewoo,",
        "Akka jaalala Yesus",
        "Takka jaalalli hin turre."
      ]
    }
  ]
}