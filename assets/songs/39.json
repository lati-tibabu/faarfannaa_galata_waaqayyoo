{
  "number": 39,
  "title": "Ilaalaa hoolaa Waaqayoo",
  "category": "salvation",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Ilaalaa hoolaa Waaqayoo",
        "Isaa nuuf jedhe du'e,",
        "Inni guddaa dhama'ewoo",
        "Siin immoo fayyisuudhaf,",
        "Siin immoo siin immoo",
        "Siin immoo inni in waama."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Inni namootaaf du'eera,",
        "Cubbuu isaaniis baateera,",
        "Hundumaa ofitti in waama,",
        "Siin immoo siin immoo",
        "Siin immoo inni in waama."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Inni cubbuu keenya baatee,",
        "Nuufis jedhee dhiphate",
        "Abbaa isaatif abboomamee,",
        "Siin immoo inni fure,",
        "Siin immoo siin immoo",
        "Siin immoo inni fure."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Egaa amma fayyitteetta,",
        "Ayyaanas argatteetta,",
        "Si achi sonaan of eeggadhu,",
        "Yesus dhuunfa kee ti'oo,",
        "Siif immoo siif immoo",
        "Yesus dhuunfa kee ti'oo."
      ]
    },
    {
      "type": "VRS5",
      "lines": [
        "Yesus dafee dhufuuf jira,",
        "Isa biras in teessa,",
        "Egaa homaa hin yaadda'in,",
        "Qannoon siif qophaa'eera,",
        "Siif immoo siif immoo",
        "Qannoon siif qophaa'eera"
      ]
    }
  ]
}
