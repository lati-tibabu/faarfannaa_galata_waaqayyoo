{
  "number": 257,
  "title": "Jireenya darbuuf jettee",
  "category": "worship",
  "sections": [
    {
      "type": "CHR",
      "lines": [
        "Jireenya darbuuf jettee",
        "Mallattoo kee hin gatin,",
        "Biyya lafaa duukaa buutee",
        "Gooftaa kee hin gaddisiisin."
      ]
    },
    {
      "type": "VRS1",
      "lines": [
        "Inni ati amanattuu hadhaa'aa",
        "In mi'eessaa,",
        "Waammadhu mee maqaa isaa",
        "Si baasaa rakkina keessaa,",
        "Amantii keetti obsi",
        "Waanjoon yoo sitti ulfaate,",
        "Inni si dandeessisa",
        "Isaa kan si waammate."
      ]
    },
    {
      "type": "CHR",
      "lines": [
