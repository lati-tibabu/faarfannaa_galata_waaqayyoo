{
  "number": 173,
  "title": "Yaa namaa kan dhamaatu,",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yaa namaa kan dhamaatu,",
        "Waaqayyoon kan hin beekne,",
        "Ariifataa deebi'i",
        "Gara arjaa fayyisaatti."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Dubbiisaa dhaggeeffadhu,",
        "Kottu jedhee siwaamaa,",
        "Maaloo jarjartee hin fiigduu,",
        "Gara arjaa fayyisaatti"
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Dafii amma yeroodhaan,",
        "Ayyaana Yesus fudhu,",
        "Ariifadhuu deebi'ii,",
        "Gara arjaa fayyisaatti."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Yeroo malee yoo dhufte,",
        "Waaqayyo si hin simu'oo,",
        "Kanaafis amma kottu,",
        "Gara arjaa fayyisaatti."
      ]
    },
    {
      "type": "VRS5",
      "lines": [
        "Ilaa Yesus dhugaadha,",
        "Inni si mararfata,",
        "Amma gara isaa kottuu,",
        "Ayyaana isaa fudhadhu."
      ]
    }
  ]
}
