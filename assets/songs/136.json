    },
    {
      "type": "VRS3",
      "lines": [
        "Hojii Gooftaa keenyaa",
        "maaluma dubbatu,",
        "Hamma bara baraatti",
        "maqaan isaa haa ulfaatu,",
        "Yeroo hundumaattuu",
        "kan nama yaadatu,",
        "Maali Gooftaan akkanaa",
