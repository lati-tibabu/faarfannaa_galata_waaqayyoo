{
  "number": 253,
  "title": "Karaan jirenyatti geessu",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Karaan jirenyatti geessu",
        "Hundumaaf dhiphaadha,",
        "Dafii isumarra bu'i",
        "Ati yaa sanyii namaa,",
        "Cubbuun kan joonjaatu."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Iddoo kee amma illee jira,",
        "Amma iyyuu si eega,",
        "Bobaa Yesus Kiristos jala",
        "Achitti iddoon gaariin,",
        "Siif qoppheeffameera."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Achi jiru kumaatamootni",
        "Qulqulluu kan ta'an,",
        "Waaqayyo akka tola isaatti",
        "Iddoo gaarii tokko siifis kaa'ee jira."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Egaa yaa nama cubbamaa",
        "Gara dhugaa kottu,",
        "Iddoo kee sonaan eeggadhu,",
        "Karicha irraa kaattee",
        "Akka isa hin dhanetti."
      ]
    },
    {
      "type": "VRS5",
      "lines": [
        "Gooftaa Yesus fayyisaa ko",
        "Siin galateeffadha,",
        "Waan iddoo naaf qopheessiteef,",
        "Achis nan jiraadha nagaas nan argadha."
      ]
    },
    {
      "type": "VRS6",
      "lines": [
        "Nagaan ati naaf kennitus,",
        "Afaan namootaatiin",
        "Silaa dubbatamee hin dhumuu,",
        "Galata Waaqayyoof",
        "Galchi yaa lubbuu ko."
      ]
    }
  ]
}
