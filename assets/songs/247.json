{
  "number": 247,
  "title": "Yaa namummaa ko",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yaa namummaa ko",
        "Jabaadhuu loli,",
        "Inni si jibbu dhi'oodhaa,",
        "Si liqimsuus in barbaadaa,",
        "Egaa of eegi dammaqii loli."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Biyya kee ilaali isaa waaqa irraa,",
        "Amma jabaattee yoo lolte,",
        "Daftee isa bira in geessa,",
        "Gooftaa kee arguutti sonaan gammadi."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "3 Waan gaarii qabda biyya",
        "Jireenyaatti,",
        "Gooftaan kee kan siif qopheesse,",
        "Egaa akka isa hin dhabnetti,",
        "Jabaadhu Waaqayyoon duukaa bu'i."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Dammaqii loli barri yartuudhaa,",
        "Daftee biyya kee in argitaa,",
        "Nagaa fi qannootti achi jiraatta,",
        "Baruma baraan egaa in boqotta."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "3 Waan gaarii qabda biyya",
