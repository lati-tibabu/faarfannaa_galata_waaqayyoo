{
  "number": 163,
  "title": "Yaa garaa ko dammaqi,",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yaa garaa ko dammaqi,",
        "Yesusiinis faarfadhu,",
        "Waan guddaa siif hojjetee,",
        "Faayyisaa kee faarfadhu"
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Ani nagaadhaan nan rafa,",
        "Innoo lubbuu koo in eega,",
        "Seexanni na kajeelus,",
        "Yesus garuu na oolcha."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Egaa eenyu tuqaree,",
        "Isa Waaqayyo eegu,",
        "Sodaa malee in rafa,",
        "Bobaa Abbaa isaa jala."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Abbaan isaas isaan in jedha,",
        "Yaa mucaa ko boqodhu,",
        "Calluma jedhi rafi,",
        "Hamma aduttiin baatutti."
      ]
    }
  ]
}