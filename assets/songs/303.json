{
  "number": 303,
  "title": "Ergamoota wajjin",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Ergamoota wajjin",
        "Yesusin faarfanna,",
        "Ulfina isaas",
        "inni iddoo mo'utti",
        "Ergamootni in jaju,",
        "Ayyaana nuuf kenna",
        "Mootiin keenya"
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Inni arrabsamee",
        "nuufis fannifamee,",
        "Dhibee argee",
        "Ergamoonni arganii",
        "Baayiees dinqifatan.",
        "Lubbuu koo amani",
        "Innoo kan kee tii."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Nagaan isaa guddaa dha,",
        "Garuu kadhaan keenya",
        "hamma qaba,",
        "Guyyaa booddee garuu isa in faarfanna,",
        "Garaa qullaa'aa dhaan",
        "Achi waaqa irratti."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Lafa gubbaatti immoo",
        "isa in faarfanna,",
        "Gooftaa keenya,",
        "Humni isaa guddaa dha,",
        "Kophaa isaas moootiidha,",
        "Kan isa fakkaatu hin argamu."
      ]
    }
  ]
}