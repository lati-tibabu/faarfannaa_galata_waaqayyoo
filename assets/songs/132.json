{
  "number": 132,
  "title": "Ameen, Haalleeluyyaa!",
  "sections": [
    {
      "type": "CHR",
      "lines": [
        "Ameen, Haalleeluyyaa!",
        "Waaqayyoof ulfinni haa ta'u",
        "Waaqaa fi lafa irratti"
      ]
    },
    {
      "type": "VRS1",
      "lines": [
        "Biiftuunii fi jiini Urjoonni hundinuu",
        "kan sagadaniif, Raajotni, ergamootni toloonnii fi",
        "Qulqulloonni kan faarfatan,",
        "Uumamni hundinuu Hojiin harka isaa",
        "dhugaa ba'uuf, Ulfina kennuuf Gooftaa jabaadhaaf",
        "Isa jiraataaf."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Ameen, Haalleeluyyaa!",
        "Waaqayyoof ulfinni haa ta'u",
        "Waaqaa fi lafa irratti"
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Biyyi Lafaa kun amanuu didee",
        "Yoo Gooftaa gane,",
        "Waaqayyo jabaadha seexana in balleessa",
        "Inni hin raafamu,",
        "Gooftaa amanamaan",
        "ofii isaa hin ganu cimee jiraata,",
        "Warra amananiif kan abdataniif Jireenya laata."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Ameen, Haalleeluyyaa!",
        "Waaqayyoof ulfinni haa ta'u",
        "Waaqaa fi lafa irratti"
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Aalfaa Oomeegaa kan turee fi kan jiru",
        "kan jiraatuu dha.",
        "Mootiin uumamaa angafni nagaa",
        "Gooftaa jabaa dha,",
        "Waaqayyo humnisaa yaada namaatiin",
        "hin safaramu,",
        "Cimee jiraata baruma baraa",
        "hin geddaramu."
      ]
    },
