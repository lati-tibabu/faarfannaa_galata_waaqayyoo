{
  "number": 205,
  "title": "Ani jiraataa dha jedha",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Ani jiraataa dha jedha",
        "Waaqayyoo uumaan hundumaa,",
        "Du'a cubbamaa ani hinfedhu,",
        "Qalbii diddiirradhu jedha,",
        "Waaqayyo araara qabeessa,",
        "Inni cubbuu kee sif dhiisa,",
        "Qalbii diddiirradhu jedhee,",
        "Wangeela keesatti in ibsa."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Yaa lafaa yaa lafaa dhaga'i,",
        "Dubbii Waaqayyoo dhaga'i,",
        "Hundee jireenyaa bari."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Dubbii Waaqayyoo dhaga'uun",
        "Du'aa gara jireenyaatti,",
        "Foonii fi lubbuun gargar ba'ee",
        "Guyyaa adda adda ta'utti,",
        "Waaqayyotti kan jiraatu",
        "Akka dubbii dhugaa isaatti,",
        "Gaafa hundaa gammachuu qaba",
        "As ta'u biyya haaraattis."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Yaa lafaa yaa lafaa dhaga'i,",
