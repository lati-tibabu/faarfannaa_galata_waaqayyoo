{
  "number": 294,
  "title": "Akka Yaa'iqoob eebbaa",
  "category": "praise",
  "sections": [
    {
      "type": "CHR",
      "lines": [
        "Akka Yaa'iqoob eebbaa",
        "fi araara keen,",
        "Na eebbisi maaloo",
        "adara kee,",
        "Nan amana hunda",
        "akka dandeessu,",
        "Na eebbifu malee gadii",
        "si hin dhiisu."
      ]
    },
    {
      "type": "VRS1",
      "lines": [
        "Yoo fedha kee jilbi",
        "koo haa dhukkubu,",
        "Hammaa dhumaatti imimmaan",
        "koo haa roobu,",
        "Yoo fedha kee hammaa",
        "bariitti illee,",
        "Gadi si hin dhiisu",
        "na eebbiftu malee."
      ]
