{
  "number": 13,
  "title": "Dhufa Keetiin Yesus",
  "category": "gospel",
  "sections": [
    {
      "type": "CHR",
      "lines": [
        "Dhufa Keetiin Yesus",
        "gooftummaa du'aa nu oolchite",
        "Galata Kee aangoo du'aa",
        "kan cabsite"
      ]
    },
    {
      "type": "VRS1",
      "lines": [
        "Yesus ko,",
        "Waaqayyo dhugaa kan turte,",
        "Duri durii Waaqayyo Abbaa",
        "wajjin jiraatee,",
        "Akka abdichaatti",
        "Maariyaam Durbattii dhalattee,",
        "Nuuf jettee immoo dhuftee",
        "nama dhugaa taate"
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Dhufa Keetiin Yesus",
        "gooftummaa du'aa nu oolchite",
        "Galata Kee aangoo du'aa",
        "kan cabsite"
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Waa'ee dhufaatii kee",
        "kan himu sagalee labsii,",
        "Lafa duwwaatti kan ergame Yohannis,",
        "Akkas jedhe,",
        "Yesus na booddeen in dhufa,",
        "Mootummaan isaan",
        "Innoo akka biiftuu in ifa."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Dhufa Keetiin Yesus",
        "gooftummaa du'aa nu oolchite",
        "Galata Kee aangoo du'aa",
        "kan cabsite"
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Yesus, dhuftee",
        "Nama gidduu deddeebitee,",
        "Fayyisuu keen uummata kee",
        "jabeesitee,",
        "Bartoota kee Wangeela barsiiftee,",
        "Seexana irratti gooftummaa",
        "isaaniif laatte."
      ]
    },
