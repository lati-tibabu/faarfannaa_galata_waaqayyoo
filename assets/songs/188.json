{
  "number": 188,
  "title": "Gammachuun nan deema",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Gammachuun nan deema",
        "Waamicha Mootichaa,",
        "Dukkanni achi hin jiru",
        "Ifsaan isaa hoolicha."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Yaaadni biyya lafaa",
        "dhiphinaa fi boo'icha,",
        "Gooftaan nuttii in haqa",
        "Imimmaan boo'ichaa."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Biyyi koo fagoo dha",
        "yooman achi ga'a?",
        "Yesusii wajjiniin",
        "gammachuudhaan taa'a"
      ]
    }
  ]
}