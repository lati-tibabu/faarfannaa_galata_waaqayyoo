{
  "number": 281,
  "title": "Borofni burqaa bishaani akkuma yaadu/2/",
  "category": "worship",
  "sections": [
    {
      "type": "CHR",
      "lines": [
        "Borofni burqaa bishaani akkuma yaadu/2/",
        "Garaan koo si dheeboteera,",
        "dheebudhaan hafuu hin barbaadu/2/",
        "teessoo kee arguu hawweera"
      ]
    },
    {
      "type": "VRS1",
      "lines": [
        "Iddo bultii kee yaa Gooftaa",
        "yooman argaa laata,",
        "Yommuun biyya olii yaadu",
        "garaan koo achi hawwa",
        "humna koon arguu hin danda'uu",
        "amantiin na dhaabi",
        "Kottuu gaaras na baasi",
        "harka koo na qabii."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Borofni burqaa bishaani akkuma yaadu/2/",
        "Garaan koo si dheeboteera,",
        "dheebudhaan hafuu hin barbaadu/2/",
        "teessoo kee arguu hawweera"
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Lubbuun koo yeroo hundaa",
        "Si yaaddi yooman arga jettee,",
        "Teessoo kee jala jiraachuuf",
        "guyyicha dharraatee,",
        "Fuula fayyisaa kpoo argee",
        "gammachuun gammada,",
        "Biyya lafaa fi rakkina isaas",
        "nan irraanfadha."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Borofni burqaa bishaani akkuma yaadu/2/",
        "Garaan koo si dheeboteera,",
        "dheebudhaan hafuu hin barbaadu/2/",
        "teessoo kee arguu hawweera"
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Qulqullonni Waaqa irra jiraatan",
        "kufanii sagadu,",
        "Dheebuun koo naafis isa kana",
        "lubbuun koo kan yaadu,",
        "Ergamoota isaa wajjin",
        "hawwii koo isaa wajjin",
        "hawwii koo isa ta'e,",
        "Anis yooman sagada laata",
        "addan lafa dha'ee."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Borofni burqaa bishaani akkuma yaadu/2/",
        "Garaan koo si dheeboteera,",
        "dheebudhaan hafuu hin barbaadu/2/",
        "teessoo kee arguu hawweera"
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Na jabeessi yaa gooftaa koo",
        "fuula kee akkan argu,",
        "anoo nan dhiisa raawadhee",
        "gammachuu isa darbu,",
        "Amma egaa na ga'eera",
        "foon koo dheebu hin baasu,",
        "Atis anaaf gaachana'oo",
        "homtuu na hin naasisu."
      ]
    },
