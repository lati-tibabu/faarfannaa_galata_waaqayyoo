      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Dukkana isa guddaa",
        "ifatti diddiiree,",
        "Dhagoota jajjaboo",
        "harootti geeddaree,",
        "Ulfina isaatiif lafti in socho'e,",
        "Galaanni Yordaanos",
        "booddeesaa deebi'e."
      ]
    },
