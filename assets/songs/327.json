  "sections": [
    {
      "type": "CHR",
      "lines": [
        "Galateeffadhaa, (4)",
        "Isin garboonni isaa",
        "maqaa isaa faarfadhaa,",
        "Inni kophaa isaa dinqii kan godhu,",
        "Waaqayyo    Gooftaa dhaa."
      ]
    },
    {
      "type": "VRS1",
      "lines": [
        "Araara isa guddaan",
        "saba isaa baayisee,",
        "Diinota isaanii irratti",
        "irree isaa mul'isee",
        "Yeroo boo'ichaatii fi dhiphinaa",
        "dabarsee,",
        "Ilillee fi gammachuutti",
        "ijoollee isaa baase."
      ]
    },
    {
      "type": "CHR",
      "lines": [
