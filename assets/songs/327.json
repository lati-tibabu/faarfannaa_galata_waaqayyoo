    {
      "type": "VRS4",
      "lines": [
        "Gammachuudhaan maqaa",
        "isaa jajadhaa,",
        "Tokko utuu hin hambisin",
        "gochaa isa dubbadhaa,",
        "Kan inni hojjete dinqii isa",
        "yaada dhaa,",
        "Garaa keessan hundaan",
        "maqaa isaa faarfadhaa."
