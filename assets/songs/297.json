{
  "number": 297,
  "title": "Waaqayyo gaarii nuuf godhe,",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Waaqayyo gaarii nuuf godhe,",
        "Ayyaana isaan nnu golboobe",
        "Kakuu isaa durii Yaadatee,",
        "Jireenya nagaa nuuf laate.",
        "Garaan koo hooqubaa dhiyeessa,",
        "Gooftaa kootiifis nan weeddisa",
        "Araarri isaa amma dhumaatti,",
        "Kan isa sodaatan irratti."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Gooftaan tola isaa mul'ise",
        "Isa dur nu abdachiise",
        "kan gadi deebi'an ol kaasee,",
        "Dirriba isaa nutti uffise."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Garaan ko..."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Waaqayyo ofitti nu araarsee,",
        "Gaarummaa nu argarsiise,",
        "Irree fayyinaa diriirsee",
        "Lubbuu keenya gammachise"
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Garaan ko..."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Gooftaaf galata dhi'eessaa,",
        "Gochaa isaa hunda odeessaa,",
        "Garaa guutuudhaan faarfadhaa,",
        "Gaarummaa isaa immoo yaadadhaa."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Garaan ko..."
      ]
    },
    {
      "type": "OTR",
      "lines": [
        "Garaa ko..."
      ]
    }
  ]
}