{
  "number": 269,
  "title": "Yaa Yesus ilma Waaqayyoo,",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yaa Yesus ilma Waaqayyoo,",
        "Karaa hunduma keenyaa,",
        "Daandii qajeelaa gubbaa irra,",
        "Gaaf hundumaa nu geessi."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Ati gammachuu fi dhugaa keenya,",
        "Kan akka kees hin jiru,",
        "Ati ifa namummaa keenyaa ti,",
        "Bara keenya hundumaa."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Atis nuuf jireenya keenya,",
        "Sitti amma kan amane,",
        "Inni jireenya in argata,",
        "Gulanas hin yeella'u."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Egaa karaanii fi jireenyi,",
        "Simalee hin argamu,",
        "Yaa Yesus Kiristos Gooftaa ko,",
        "Karaa kee nu barsiisi."
      ]
    }
  ]
}