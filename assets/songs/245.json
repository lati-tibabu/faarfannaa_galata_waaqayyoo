{
  "number": 245,
  "title": "Biyyi lafaa kun in darba,",
  "category": "worship",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Biyyi lafaa kun in darba,",
        "Badhaadhummaan isaas hundinuu,",
        "Dubbiin Waaqayyoo hin darbu,",
        "Bara baraan jiraataa dha."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Biyyi keenyasoo Waaqaa irraa,",
        "Lafti adbii dhaala keenyaa,",
        "Asii hin qabnu lafa dhuunfaa,",
        "Keessummummaa dhaanoo",
        "Teenyaa."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Godoon keenya inni lafa irraa,",
        "Jijjigee baduufoo jiraa,",
        "Kan hin jigne mana qabna,",
        "Waaqa irraa kan bara baraa."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Gaddi yookiis yaaddoon illee,",
        "Gonkumaa kan keessa hin jirre,",
        "Hamma bara baraatti illee,",
        "Eenyutu isa argata ree!"
      ]
    },
    {
      "type": "VRS5",
      "lines": [
        "Dhugaa Yesusiif jedhanii,",
        "Sonaan kan dhiphatan malee,",
        "Gidiraa hundaa obsanii,",
        "Fannoo isaanii baatan malee."
      ]
    },
    {
      "type": "VRS6",
      "lines": [
        "Gaafasis attam gammadu",
        "Gooftaa kan amanatanuu",
        "Du'aa jireenyatti in darbu",
        "Firdiin isaa hin argatu"
      ]
    },
    {
      "type": "VRS7",
      "lines": [
        "Guyyaan Firdii hin oolu in dhufa,",
        "Kottaa hin rafnuu in dammaqna,",
        "Gatii isaa in argata hunduu,",
        "Gaafas firri nama hin oolchu."
      ]
    }
  ]
}
