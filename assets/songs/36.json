  "title": "Dhiiga kee naaf dhangalaaftee,",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Dhiiga kee naaf dhangalaaftee,",
        "Ana cubbamaaf yakkamte,",
        "Dhiphina hundaa obsitee,",
        "Iddoo yeelloo naaf dhaabattee,",
        "Gochaan kee guddaadha",
        "Gooftaa ko,",
        "Humna kee naaf kenni",
        "Jajjabeessaa ko."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Si arguuf,(3)",
        "Si arguuf yaa Yesus,",
        "Baay’een ariifadha,",
        "Humna dhufa keetti",
        "Humna kee naaf kenni",
        "Obsaan qabaadhaa."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Gooftaa ko anaaf du’uu kee,",
        "Dagadheen oole gochaa kee,",
        "Gaara Golgootaa gubbaatti,",
        "Kan ati naaf taateen yaadadhe,",
        "Garaan koo na didee siin dhiisuu,",
        "Humna kee naaf kenni akkan",
        "dhaabadhuuf.",
        "Si arguuf ..."
      ]
    },
