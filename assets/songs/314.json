      "type": "CHR",
      "lines": [
        "Mee kaanee ijaarra",
        "masaricha keenya,",
        "Gooftaan nu gargaara yaa",
        "obboloota keenyaa,",
        "Nu'i garboota isaati mi'a",
        "Gooftaa keenyaa, (2)",
        "Waaqayyo jabaadha innoo nu gargaara"
      ]
    },
