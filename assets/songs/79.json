      "lines": [
        "Yesuus deebi'uudhaaaf",
        "yeroon isaa ga'eera,",
        "Dubbiin Gooftaa keenyaa",
        "hunduu raawwateera,/2/"
      ]
    },
    {
      "type": "VRS1",
      "lines": [
        "Mootichi moototaa mul'achuuf",
        "Hojiin biyya lafaa darbuudhaf,",
        "Aduun bara dhumaa lixuuf."
      ]
