{
  "number": 226,
  "title": "Yaa Gooftaa biyya gogaa dhaaf,",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yaa Gooftaa biyya gogaa dhaaf,",
        "Ayyaana ergiteetta,",
        "Bokkaa danuus roobsiteetta,",
        "Anaafis immoo roobsi",
        "Anaaf, *2 anaafis immoo roobsi."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Yaa Abbaa na hin irraanfatin,",
        "Ani cubbuu hojjedheeraa,",
        "Maaloo anaaf araarami,",
        "Ayyaana kee naaf kenni,",
        "Anaa immoo, anaa immoo,",
        "Ayyaana kee naaf kenni."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Yaa Yesus na bira hin darbin,",
        "Irree keetti na baadhadhu,",
        "Kottuu na keessa jiraadhu,",
        "Jireenya koo dhuunfadhu,",
        "Ana immoo, ana immoo,",
        "Dhuunfaa kee na godhadhu."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Hafuura Qulqulluu kottu,",
        "Ati ifsaa kee naaf ifsi,",
        "Amantii dhugaas naaf kenni,",
        "Jabaadhe akkan lolutti,",
        "Ani immoo, ani immoo,",
        "Jabaadhee akkan lolutti."
      ]
    },
    {
      "type": "VRS5",
      "lines": [
        "Jaalala kee na barsiisii,",
        "Cubbuuttii na qulleessi,",
        "Humnaa fi ayyaana naaf ergi,",
        "Si duukaa akkan bu'utti,",
        "Ani immoo, ani immoo,",
        "Si duukaa akkan bu'utti."
      ]
    },
    {
      "type": "VRS6",
      "lines": [
        "Yaa Waaqayyoo na harkisi,",
        "Ofitti na dhi'eessi,",
        "Yommuu kan biraa obaaftu,",
        "Lubbuu koo immoo naaf jiisi,",
        "Ana immoo, ana immoo,",
        "Lubbuu koo immaa naaf jiisi."
      ]
    }
  ]
}