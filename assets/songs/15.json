{
  "number": 15,
  "title": "Furiin keenya argamee,",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Furiin keenya argamee,",
        "Ilmi mootii Guddaa",
        "Gara keenya dhufeera,",
        "Luba nu baaseera,",
        "akka urjii bariidhaa,",
        "Keessa keenya in ibsa,",
        "Dukkana in fageessa."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Inni nama ta'eera,",
        "Hundaaf kennameera,",
        "Guyyaan eebbaa dhufeera,",
        "Nagaas nuuf fideera,",
        "Humni du'aa cabeera,",
        "Hidhaan cubbuu citeera,",
        "Waaqnis banameera."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Yaa Kiristos nuuf birmadhu,",
        "Amantii nuuf laadhu,",
        "Ifa akka arginutti,",
        "Akka si jaallannutti,",
        "Garaa keenyaan si yaadna,",
        "Biyya dhibamaa kanaa",
        "Gara kee hawwina."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Abbaan kee siif kenneera,",
        "Harka keetti waan hundaa,",
        "Walittis in qabatta,",
        "Jaalalaan obaafta,",
        "Hoolota kee godhatta,",
        "Warra galateeffaman,",
        "Kan si duukaa bu'an."
      ]
    },
    {
      "type": "VRS5",
      "lines": [
        "Abbaan kee kan siif kenne,",
        "Atis hin daganne,",
        "Jireenya hin darbine,",
        "Isa in quufsite,",
        "Adaraa anas yaadadhu,",
        "Bara koo hundumaattuu",
        "Jireenya naaf laadhu."
      ]
    }
  ]
}