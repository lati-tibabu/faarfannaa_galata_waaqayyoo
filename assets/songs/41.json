{
  "number": 41,
  "title": "Yesus malee eenyunni ree",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yesus malee eenyunni ree",
        "Hoolan mudaa hin qabne",
        "Golgootattis kan qalame",
        "Hundaafa kan aarfame"
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Inni nuf jedhee yakkame",
        "Muddamee dhiphatee",
        "Ba’aa cubbuu keenyaa baatee",
        "Fannootti ol baafame"
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Achi irrattis waraanamee",
        "Dhiiga isaa lolaase",
        "Nuuf raawwate Jaalala isaa",
        "Kennekaa lubbu isaa"
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Kan akka Yesus jiraaree",
        "Dhiiga isaa lolaasee",
        "Lubbuu hundaa furuuf jedhee",
        "Du’aatti kan of kenne"
      ]
    },
    {
      "type": "VRS5",
      "lines": [
        "Kan akka isaa hin argannu",
        "Gonkumaa eessattuu",
        "Barbaannuyyuu hin argannu",
        "akkas kan of kennu"
      ]
    }
  ]
}