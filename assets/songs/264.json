  "title": "Qoraattiin latee na hudhuu",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Qoraattiin latee na hudhuu",
        "Ga'eera,",
        "Raareen cubbuudhaa na",
        "Liqimsee jira,",
        "Kana ooluudhaaf homtinuu na",
        "hin gargaaru,",
        "Human keen malee tokko illee",
        "hin hoofkalu."
      ]
    },
    {
      "type": "CHR",
      "lines": [
        "Kottu kottu yaa Yesus gara koo",
        "Kottu,Kottu yaa Yesus gara koo,",
        "Cubbuu koo hunda",
        "Naaf dhiisi Gooftaa ko,",
        "Hafuura keetiin haaressi",
        "Namummaa koo."
      ]
    },
