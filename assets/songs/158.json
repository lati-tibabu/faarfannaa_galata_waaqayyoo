{
  "number": 158,
  "title": "Yesus Kiristos yeroo sodaa fi",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Yesus Kiristos yeroo sodaa fi",
        "dadhabbiitti nu jabeessa,",
        "Guyyaa guyyaadhaan nuun jedha,",
        "Kottaa na duukaa bu'aa."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Yeroo gaddaa fi gammachuuttis,",
        "Yeroo hojii fi boqochuuttis",
        "Rakkoo hundumaa keessattis,",
        "Gooftaan nu jajjabeessa."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Yaa Gooftaa ati nu waamta,",
        "Dhaga'uuf nu gargaari,",
        "Siif hojjechuuf si jaallachuuf,",
        "Siif abboomamuudhaaf."
      ]
    }
  ]
}