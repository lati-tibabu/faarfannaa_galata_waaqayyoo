{
  "number": 234,
  "title": "Yaa Gooftaa fulduraan",
  "category": "worship",
  "sections": [
    {
      "type": "CHR",
      "lines": [
        "Yaa Gooftaa fulduraan",
        "dungoonuuf ta'i,",
        "Yaa Yesus booddeedhaan",
        "Irkoo nuf ta'i,",
        "Biyyattii isheen waaqa",
        "irraan nu ga'i."
      ]
    },
    {
      "type": "VRS1",
      "lines": [
        "Yommuun rakkachaa ture,",
        "Jireenya barbaacha jooree,",
        "Jireenyi kee anumaa kottu",
        "mucaa ko naan jette."
      ]
    },
