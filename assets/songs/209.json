{
  "number": 209,
  "title": "Ka'i ka'i Yesusiif",
  "sections": [
    {
      "type": "VRS1",
      "lines": [
        "Ka'i ka'i Yesusiif",
        "Ati yaa loltuu fannoo,",
        "Milikkita mo'ichaa",
        "Mul'iftee ol fuudhi,",
        "Mo'aa gara mo'ichaatti",
        "Loltuun isaa in kaatu,",
        "Diina hamma cabsanutti",
        "Maqaa Gooftichaatiin."
      ]
    },
    {
      "type": "VRS2",
      "lines": [
        "Ka'i ka'i Yesusiif",
        "Fiingeenoo si waamaa,",
        "Galata Gooftaa himuuf",
        "Seexanas ari'achuuf,",
        "Diinotni yoo jabaatan",
        "Homaa iyyuu hin sodaatin,",
        "Ati jagnoomi malee",
        "Amaanu'eel siif jira."
      ]
    },
    {
      "type": "VRS3",
      "lines": [
        "Ka'i ka'i Yesusiif",
        "Humna isaa duwwaadhaan,",
        "Irreen foonii si hin baasu",
        "Jabaa illee ta'e,",
        "Mi'a olii gonfadhu",
        "Dammaqxees kadhadhu,",
        "Gara jara lolaniitti",
        "Adeemi siin jedha."
      ]
    },
    {
      "type": "VRS4",
      "lines": [
        "Ka'i ka'i Yesusiif",
        "Lollii jabaatu iyyuu,",
        "Har'a fiingee in dhageessaa",
        "Bor weedduu mo'ichaa,",
        "Kan diina isaa mo'ate",
        "Gatii malee hin hafuu,",
        "Waaqayyoo isaaf kenna",
        "Ulfina guddaa isaa."
      ]
    }
  ]
}